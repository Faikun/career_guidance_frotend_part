{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"Comments\",\n  setup() {\n    const today = new Date();\n    const yyyy = today.getFullYear();\n    let mm = today.getMonth() + 1; // Months start at 0!\n    let dd = today.getDate();\n    if (dd < 10) dd = '0' + dd;\n    if (mm < 10) mm = '0' + mm;\n    const formattedToday = dd + '/' + mm + '/' + yyyy;\n    let putLike = function (comment, userId) {\n      if (comment.some(data => data.usersIdLikes === userId)) {\n        console.log(comment.usersIdLikes);\n        console.log(comment.some(data => data.usersIdLikes === userId));\n        comment.usersIdLikes.push(userId);\n      } else comments.value.splice(comments.value.findIndex(comment), 1);\n    };\n    let putDislike = function (comment, userId) {\n      comment.usersIdDislikes.push(userId);\n    };\n    let comments = ref([{\n      user: 'username',\n      date: formattedToday,\n      stars: 4,\n      comment: 'Хороший тест!',\n      usersIdLikes: [1, 2],\n      usersIdDislikes: []\n    }]);\n    return {\n      comments,\n      putLike,\n      putDislike\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","today","Date","yyyy","getFullYear","mm","getMonth","dd","getDate","formattedToday","putLike","comment","userId","some","data","usersIdLikes","console","log","push","comments","value","splice","findIndex","putDislike","usersIdDislikes","user","date","stars"],"sources":["E:\\Vue\\career_guidance\\src\\components\\Comments.vue"],"sourcesContent":["<template>\r\n<div class=\"comments\">\r\n  <h2 class=\"comments__title\">Поделитесь со своми впечатлениями (1)</h2>\r\n  <div class=\"comments__leaveAComment\">\r\n    <span class=\"comments__stars\">\r\n      <i>★</i><i>★</i><i>★</i><i>★</i><i>★</i>\r\n    </span>\r\n    <div class=\"comments__input\">\r\n      <input class=\"comments__input-input\" type=\"text\" placeholder=\"Напишите комментарий\">\r\n      <img class=\"comments__input-img\" src=\"../images/png/ArrowToRightBlue.png\" alt=\"arrow\">\r\n    </div>\r\n  </div><!-- ./comments__leaveAComment -->\r\n  <div class=\"comments__list\">\r\n    <div class=\"comments__item\" v-for=\"comment in comments\" :key=\"comment.date\">\r\n      <div class=\"comments__user\">\r\n        <div class=\"comments__user-name\">{{ comment.user }}</div>\r\n        <div class=\"comments__user-date\">{{ comment.date }}</div>\r\n        <div class=\"comments__user-stars\">\r\n          <img src=\"../images/png/Star-black.png\" alt=\"star\" v-for=\"item in comment.stars\">\r\n        </div>\r\n      </div><!-- ./comments__user -->\r\n      <div class=\"comments__item-text\">{{ comment.comment }}</div>\r\n      <div class=\"comments__item-interaction\">\r\n        <div class=\"comments__item-answer\">Ответить</div>\r\n        <div class=\"comments__item-response\">\r\n          <div class=\"comments__item-likes\">\r\n            <img @click=\"putLike(comment, 3)\" class=\"comments__item-interactionImage\" src=\"../images/png/mdi_like.png\" alt=\"likes\">\r\n            <span>{{ comment.usersIdLikes.length }}</span>\r\n          </div>\r\n          <div class=\"comments__item-dislikes\">\r\n            <img  @click=\"putDislike(comment, 3)\" class=\"comments__item-interactionImage\" src=\"../images/png/mdi_dislike.png\" alt=\"dis\">\r\n            <span>{{ comment.usersIdDislikes.length }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div><!-- ./comments__item -->\r\n  </div><!-- ./comments__list -->\r\n</div><!-- ./comments -->\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\n\r\nexport default {\r\n  name: \"Comments\",\r\n  setup () {\r\n    const today = new Date();\r\n    const yyyy = today.getFullYear();\r\n    let mm = today.getMonth() + 1; // Months start at 0!\r\n    let dd = today.getDate();\r\n\r\n    if (dd < 10) dd = '0' + dd;\r\n    if (mm < 10) mm = '0' + mm;\r\n\r\n    const formattedToday = dd + '/' + mm + '/' + yyyy;\r\n\r\n    let putLike = function (comment, userId) {\r\n      if(comment.some(data => data.usersIdLikes === userId)) {\r\n        console.log(comment.usersIdLikes);\r\n        console.log(comment.some(data => data.usersIdLikes === userId))\r\n        comment.usersIdLikes.push(userId);\r\n      }\r\n\r\n      else\r\n        comments.value.splice(comments.value.findIndex(comment), 1)\r\n    };\r\n\r\n    let putDislike = function (comment, userId) {\r\n      comment.usersIdDislikes.push(userId)\r\n    };\r\n\r\n    let comments = ref([\r\n      {\r\n        user: 'username',\r\n        date: formattedToday,\r\n        stars: 4,\r\n        comment: 'Хороший тест!',\r\n        usersIdLikes: [1, 2],\r\n        usersIdDislikes: [],\r\n      },\r\n    ]);\r\n    return {\r\n      comments,\r\n      putLike,\r\n      putDislike\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAyCA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAI,IAAIC,IAAI,EAAE;IACxB,MAAMC,IAAG,GAAIF,KAAK,CAACG,WAAW,EAAE;IAChC,IAAIC,EAAC,GAAIJ,KAAK,CAACK,QAAQ,EAAC,GAAI,CAAC,EAAE;IAC/B,IAAIC,EAAC,GAAIN,KAAK,CAACO,OAAO,EAAE;IAExB,IAAID,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAC1B,IAAIF,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAE1B,MAAMI,cAAa,GAAIF,EAAC,GAAI,GAAE,GAAIF,EAAC,GAAI,GAAE,GAAIF,IAAI;IAEjD,IAAIO,OAAM,GAAI,SAAAA,CAAUC,OAAO,EAAEC,MAAM,EAAE;MACvC,IAAGD,OAAO,CAACE,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,YAAW,KAAMH,MAAM,CAAC,EAAE;QACrDI,OAAO,CAACC,GAAG,CAACN,OAAO,CAACI,YAAY,CAAC;QACjCC,OAAO,CAACC,GAAG,CAACN,OAAO,CAACE,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACC,YAAW,KAAMH,MAAM,CAAC;QAC9DD,OAAO,CAACI,YAAY,CAACG,IAAI,CAACN,MAAM,CAAC;MACnC,OAGEO,QAAQ,CAACC,KAAK,CAACC,MAAM,CAACF,QAAQ,CAACC,KAAK,CAACE,SAAS,CAACX,OAAO,CAAC,EAAE,CAAC;IAC9D,CAAC;IAED,IAAIY,UAAS,GAAI,SAAAA,CAAUZ,OAAO,EAAEC,MAAM,EAAE;MAC1CD,OAAO,CAACa,eAAe,CAACN,IAAI,CAACN,MAAM;IACrC,CAAC;IAED,IAAIO,QAAO,GAAIrB,GAAG,CAAC,CACjB;MACE2B,IAA