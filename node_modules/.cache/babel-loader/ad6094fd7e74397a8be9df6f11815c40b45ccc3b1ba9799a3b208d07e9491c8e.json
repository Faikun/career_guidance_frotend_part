Шымкент\">Вузы Шымкента</router-link>\r\n              <router-link v-if=\"store.getters.getAuth\" :to=\"`/edu/${location()}`\" href=\"#\">Вузы в вашем городе</router-link>\r\n            </div>\r\n          </div><!-- ./nav__link -->\r\n        </nav><!-- ./nav__links -->\r\n\r\n        <div class=\"burger__item\" @click=\"toggle = !toggle\">\r\n          <button class=\"burger\" :class=\"toggle ? 'shown' : ''\" type=\"button\">\r\n            <span class=\"burger-item\">Menu</span>\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"nav__right\">\r\n          <div class=\"nav__search\" @click=\"searchActive = !searchActive\">\r\n            <img src=\"../images/png/Search.png\" alt=\"Search\">\r\n          </div>\r\n          <div class=\"nav__account\">\r\n            <img src=\"../images/png/Account.png\" alt=\"Account\" @click=\"showAccBlock\">\r\n            <div class=\"dropdown__account\" :class=\"account ? 'active' : ''\">\r\n              <a @click=\"goToAcc\">Аккаунт</a>\r\n              <a @click=\"goToHistory\">История</a>\r\n              <a href=\"#\" @click=\"exitAcc\">Выход</a>\r\n            </div>\r\n          </div>\r\n        </div><!-- ./nav__right -->\r\n      </div><!-- ./nav__wrapper -->\r\n      <transition name=\"fadeHeight\">\r\n        <div class=\"search\" v-if=\"searchActive\">\r\n          <input class=\"search__input\" type=\"text\" v-model.trim=\"search\" placeholder=\"Введите тест, профессию или вуз\">\r\n          <img class=\"search__icon\" src=\"../images/png/Search.png\" alt=\"icon\">\r\n        </div><!-- ./search -->\r\n      </transition>\r\n    </div><!-- ./container -->\r\n  </div><!-- ./nav -->\r\n\r\n  <div class=\"container\">\r\n    <notification\r\n        v-if=\"notification.openedNotification\"\r\n        :title=\"notification.title\"\r\n        :text=\"notification.text\"\r\n        :mode=\"notification.mode\"\r\n    ></notification>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {computed, ref} from \"vue\";\r\nimport store from \"@/store\";\r\nimport Notification from \"@/components/notification.vue\";\r\nimport router from \"@/router\";\r\n\r\nexport default {\r\n  name: \"TheNavbar\",\r\n  components: { Notification },\r\n  setup() {\r\n    let search = ref('');\r\n    let account = ref('');\r\n    let searchActive = ref(false);\r\n    let toggle = ref(false);\r\n    const storeGetters = computed(() => store.getters);\r\n    let notification = store.getters.getNotificationInfo;\r\n\r\n    let exitAcc = function () {\r\n      store.commit('setAuth')\r\n      account.value = !account.value;\r\n      store.commit('setNotificationInfo', { titleValue: 'Успех', text: 'Вы успешно вышли со своего аккаунта', mode: 'success'} );\r\n      store.commit('setAdmin')\r\n    }\r\n\r\n    let showAccBlock = function () {\r\n      if (store.getters.getAuth) {\r\n        account.value = !account.value;\r\n      }\r\n      else {\r\n        store.commit('toggleOpenedAuth');\r\n        document.body.style.overflow = \"hidden\";\r\n        document.documentElement.scrollTop = 0;\r\n      }\r\n    }\r\n\r\n    let goToAcc = function () {\r\n      router.push('/account');\r\n      account.value = !account.value;\r\n    };\r\n\r\n    let goToHistory = function () {\r\n      router.push('/history');\r\n      account.value = !account.value;\r\n    }\r\n\r\n    let users = computed(() => store.getters.getUsers)\r\n\r\n    let location = function () {\r\n      console.log(users.value.filter((el) => el.userId.toString() === store.getters.getUserId))\r\n      users.value.filter((el) => el.userId === store.getters.getUserId.toString()).location\r\n    }\r\n\r\n    return {\r\n      search,\r\n      searchActive,\r\n      toggle,\r\n      account,\r\n      showAccBlock,\r\n      store, storeGetters,\r\n      exitAcc,\r\n      notification,\r\n      goToAcc, goToHistory,\r\n      location\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .fadeHeight-enter-from {\r\n    max-height: 0;\r\n    opacity: 0;\r\n  }\r\n  .fadeHeight-enter-active,\r\n  .fadeHeight-leave-active {\r\n    transition: all .5s ease-in-out;\r\n  }\r\n  .fadeHeight-leave-from {\r\n    max-height: 230px;\r\n    opacity: 1;\r\n  }\r\n  .fadeHeight-leave-to\r\n  {\r\n    opacity: 0;\r\n    max-height: 0;\r\n  }\r\n\r\n</style>\r\n"],"mappings":";AA2EA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,MAAK,MAAO,UAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAEH;EAAa,CAAC;EAC5BI,KAAKA,CAAA,EAAG;IACN,IAAIC,MAAK,GAAIP,GAAG,CAAC,EAAE,CAAC;IACpB,IAAIQ,OAAM,GAAIR,GAAG,CAAC,EAAE,CAAC;IACrB,IAAIS,YAAW,GAAIT,GAAG,CAAC,KAAK,CAAC;IAC7B,IAAIU,MAAK,GAAIV,GAAG,CAAC,KAAK,CAAC;IACvB,MAAMW,YAAW,GAAIZ,QAAQ,CAAC,MAAME,KAAK,CAACW,OAAO,CAAC;IAClD,IAAIC,YAAW,GAAIZ,KAAK,CAACW,OAAO,CAACE,mBAAmB;IAEpD,IAAIC,OAAM,GAAI,SAAAA,CAAA,EAAY;MACxBd,KAAK,CAACe,MAAM,CAAC,SAAS;MACtBR,OAAO,CAACS,KAAI,GAAI,CAACT,OAAO,CAACS,KAAK;MAC9BhB,KAAK,CAACe,MAAM,CAAC,qBAAqB,EAAE;QAAEE,UAAU,EAAE,OAAO;QAAEC,IAAI,EAAE,qCAAqC;QAAEC,IAAI,EAAE;MAAS,EAAG;MAC1HnB,KAAK,CAACe,MAAM,CAAC,UAAU;IACzB;IAEA,IAAIK,YAAW,GAAI,SAAAA,CAAA,EAAY;MAC7B,IAAIpB,KAAK,CAACW,OAAO,CAACU,OAAO,EAAE;QACzBd,OAAO,CAACS,KAAI,GAAI,CAACT,OAAO,CAACS,KAAK;MAChC,OACK;QACHhB,KAAK,CAACe,MAAM,CAAC,kBAAkB,CAAC;QAChCO,QAAQ,CAACC,IAAI,CAACC,KAAK,CAACC,QAAO,GAAI,QAAQ;QACvCH,QAAQ,CAACI,eAAe,CAACC,SAAQ,GAAI,CAAC;MACxC;IACF;IAEA,IAAIC,OAAM,GAAI,SAAAA,CAAA,EAAY;MACxB1B,MAAM,CAAC2B,IAAI,CAAC,UAAU,CAAC;MACvBtB,OAAO,CAACS,KAAI,GAAI,CAACT,OAAO,CAACS,KAAK;IAChC,CAAC;IAED,IAAIc,WAAU,GAAI,SAAAA,CAAA,EAAY;MAC5B5B,MAAM,CAAC2B,IAAI,CAAC,UAAU,CAAC;MACvBtB,OAAO,CAACS,KAAI,GAAI,CAACT,OAAO,CAACS,KAAK;IAChC;IAEA,IAAIe,KAAI,GAAIjC,QAAQ,CAAC,MAAME,KAAK,CAACW,OAAO,CAACqB,QAAQ;IAEjD,IAAIC,QAAO,GAAI,SAAAA,CAAA,EAAY;MACzBC,OAAO,CAACC,GAAG,CAACJ,KAAK,CAACf,KAAK,CAACoB,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACC,MAAM,CAACC,QAAQ,EAAC,KAAMvC,KAAK,CAACW,OAAO,CAAC6B,SAAS,CAAC;MACxFT,KAAK,CAACf,KAAK,CAACoB,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACC,MAAK,KAAMtC,