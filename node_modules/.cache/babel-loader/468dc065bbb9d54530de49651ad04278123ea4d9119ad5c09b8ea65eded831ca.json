{"ast":null,"code":"import Comments from \"@/components/Comments.vue\";\nimport { useStore } from \"vuex\";\nimport { computed, onUpdated, watch } from \"vue\";\nexport default {\n  name: \"Result\",\n  components: {\n    Comments\n  },\n  setup() {\n    let store = useStore();\n    let auth = function () {\n      store.commit('toggleOpenedAuth');\n      document.body.style.overflow = \"hidden\";\n    };\n    let finalRes;\n    onUpdated(() => console.log(finalRes));\n    const isAuth = computed(() => store.getters.getAuth);\n    watch(isAuth, (newValue, oldValue) => {\n      if (newValue) {\n        const result = JSON.parse(localStorage.getItem('test'));\n        finalRes = Object.entries(result);\n      }\n    });\n    return {\n      store,\n      auth,\n      finalRes\n    };\n  }\n};","map":{"version":3,"names":["Comments","useStore","computed","onUpdated","watch","name","components","setup","store","auth","commit","document","body","style","overflow","finalRes","console","log","isAuth","getters","getAuth","newValue","oldValue","result","JSON","parse","localStorage","getItem","Object","entries"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Result.vue"],"sourcesContent":["<template>\r\n<div class=\"result\">\r\n  <h1 class=\"result__title\">Результаты</h1>\r\n  <h3 class=\"test__completed\">Поздравляем! Вы успешно сдали тест</h3>\r\n  <ul v-if=\"store.getters.getAuth\" class=\"result__show\">\r\n    <span>Итоги теста: </span>\r\n    <li v-for=\"res in finalRes\" :key=\"finalRes\">\r\n      <span>{{ res[0] }}</span>\r\n      <span>{{ res[1] }}</span>\r\n    </li>\r\n  </ul><!-- ./result__show -->\r\n  <div v-else class=\"result__auth\">\r\n    <div class=\"result__statement\">Чтобы увидеть результат, пожалуйста, пройдите регистрацию или войдите в свою учётную запись</div>\r\n    <div class=\"result__auth-links\">\r\n      <a href=\"#\" @click=\"auth\">Войти в аккаунт</a>\r\n      <a href=\"#\" @click=\"auth\">Зарегестрироваться</a>\r\n    </div>\r\n  </div><!-- ./result__auth -->\r\n  <comments></comments>\r\n</div><!-- ./result -->\r\n</template>\r\n\r\n<script>\r\nimport Comments from \"@/components/Comments.vue\";\r\nimport {useStore} from \"vuex\";\r\nimport {computed, onUpdated, watch} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Result\",\r\n  components: {Comments},\r\n  setup() {\r\n    let store = useStore();\r\n    let auth = function () {\r\n      store.commit('toggleOpenedAuth');\r\n      document.body.style.overflow = \"hidden\";\r\n    }\r\n    let finalRes;\r\n\r\n    onUpdated(() => console.log(finalRes))\r\n\r\n    const isAuth = computed(() => store.getters.getAuth)\r\n\r\n    watch(isAuth, (newValue, oldValue) => {\r\n      if(newValue) {\r\n        const result = JSON.parse(localStorage.getItem('test'));\r\n        finalRes = Object.entries(result);\r\n      }\r\n    })\r\n\r\n    return {\r\n      store,\r\n      auth,\r\n      finalRes\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAuBA,OAAOA,QAAO,MAAO,2BAA2B;AAChD,SAAQC,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,EAAEC,SAAS,EAAEC,KAAK,QAAO,KAAK;AAE9C,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IAACN;EAAQ,CAAC;EACtBO,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIP,QAAQ,EAAE;IACtB,IAAIQ,IAAG,GAAI,SAAAA,CAAA,EAAY;MACrBD,KAAK,CAACE,MAAM,CAAC,kBAAkB,CAAC;MAChCC,QAAQ,CAACC,IAAI,CAACC,KAAK,CAACC,QAAO,GAAI,QAAQ;IACzC;IACA,IAAIC,QAAQ;IAEZZ,SAAS,CAAC,MAAMa,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;IAErC,MAAMG,MAAK,GAAIhB,QAAQ,CAAC,MAAMM,KAAK,CAACW,OAAO,CAACC,OAAO;IAEnDhB,KAAK,CAACc,MAAM,EAAE,CAACG,QAAQ,EAAEC,QAAQ,KAAK;MACpC,IAAGD,QAAQ,EAAE;QACX,MAAME,MAAK,GAAIC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;QACvDZ,QAAO,GAAIa,MAAM,CAACC,OAAO,CAACN,MAAM,CAAC;MACnC;IACF,CAAC;IAED,OAAO;MACLf,KAAK;MACLC,IAAI;MACJM;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}