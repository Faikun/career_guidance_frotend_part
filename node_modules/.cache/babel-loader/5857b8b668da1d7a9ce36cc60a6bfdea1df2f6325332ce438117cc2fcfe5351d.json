 </table><!-- ./admin__wrapper -->\r\n</div><!-- ./admin -->\r\n</template>\r\n\r\n<script>\r\nimport {useStore} from \"vuex\";\r\nimport {computed, onUpdated, reactive, ref} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Admin\",\r\n  setup() {\r\n    let store = useStore();\r\n    let users = computed(() => store.getters.getUsers);\r\n    let inputDisabled = reactive([]);\r\n\r\n    onUpdated(() => {\r\n      console.log('onMounted')\r\n      for (let i = 0; i < users.value.length; i++) {\r\n        inputDisabled.push('false')\r\n      }\r\n    })\r\n\r\n    const userName = ref(''),\r\n        userMail = ref(''),\r\n        userPassword = ref(''),\r\n        userStatus = ref('');\r\n\r\n    let saveData = function (index) {\r\n      store.commit('setUsers', {\r\n        index: index,\r\n        name: userName.value[index].value,\r\n        mail: userMail.value[index].value,\r\n        password: userPassword.value[index].value,\r\n        status: userStatus.value[index].value\r\n      });\r\n      inputDisabled[index] = !inputDisabled[index]\r\n    }\r\n\r\n    let remove = function (index) {\r\n      console.log(index);\r\n      console.log(users.value);\r\n      console.log(inputDisabled);\r\n      store.commit('removeUser', index);\r\n      this.$forceUpdate();\r\n    }\r\n\r\n    return {\r\n      users,\r\n      inputDisabled,\r\n      saveData, remove,\r\n      userName, userMail, userPassword, userStatus\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";AA+EA,SAAQA,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,EAAEC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAK;AAEtD,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIP,QAAQ,EAAE;IACtB,IAAIQ,KAAI,GAAIP,QAAQ,CAAC,MAAMM,KAAK,CAACE,OAAO,CAACC,QAAQ,CAAC;IAClD,IAAIC,aAAY,GAAIR,QAAQ,CAAC,EAAE,CAAC;IAEhCD,SAAS,CAAC,MAAM;MACdU,OAAO,CAACC,GAAG,CAAC,WAAW;MACvB,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIN,KAAK,CAACO,KAAK,CAACC,MAAM,EAAEF,CAAC,EAAE,EAAE;QAC3CH,aAAa,CAACM,IAAI,CAAC,OAAO;MAC5B;IACF,CAAC;IAED,MAAMC,QAAO,GAA