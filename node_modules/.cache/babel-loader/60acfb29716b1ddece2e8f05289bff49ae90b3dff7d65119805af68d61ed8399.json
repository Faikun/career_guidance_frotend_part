\n        document.body.style.overflow = \"hidden\";\r\n        document.documentElement.scrollTop = 0;\r\n      }\r\n    };\r\n\r\n    let leaveAComment = function () {\r\n      if(store.getters.getAuth) {\r\n        if(inputComment.value.trim() !== '' && starsCount.value != 0) {\r\n          let userName = store.getters.getUsers.\r\n          filter((el) => el.userId === parseInt(store.getters.getUserId))\r\n              .map(map => map.name).toString();\r\n          comments.value.push({\r\n            user: userName,\r\n            date: formattedToday,\r\n            stars: parseInt(starsCount.value),\r\n            comment: inputComment.value,\r\n            usersIdLikes: [],\r\n            usersIdDislikes: []\r\n          })\r\n          inputComment.value = ''\r\n        }\r\n        else {\r\n          alert('Пожалуйста, напишите комментарий и оцените тест по 5-и бальной шкале')\r\n        }\r\n\r\n      }\r\n      else {\r\n        store.commit('toggleOpenedAuth');\r\n        document.body.style.overflow = \"hidden\";\r\n        document.documentElement.scrollTop = 0;\r\n      }\r\n    }\r\n\r\n    let openInput = (comment) => comment.openedInput = false;\r\n\r\n    let answerComment = function (comment) {\r\n      if(commentAnswer.value.trim() !== '') {\r\n        let userName = store.getters.getUsers.\r\n        filter((el) => el.userId === parseInt(store.getters.getUserId))\r\n            .map(map => map.name).toString();\r\n        comment.answered.push({\r\n          user: userName,\r\n          date: formattedToday,\r\n          comment: commentAnswer.value,\r\n          usersIdLikes: [],\r\n          usersIdDislikes: []\r\n        })\r\n        commentAnswer.value = '';\r\n\r\n      }\r\n      else {\r\n        alert('Пожалуйста, напишите комментарий и оцените тест по 5-и бальной шкале')\r\n      }\r\n    }\r\n\r\n    let comments = ref([\r\n      {\r\n        user: 'username',\r\n        date: formattedToday,\r\n        stars: 4,\r\n        comment: 'Хороший тест!',\r\n        usersIdLikes: [1, 2],\r\n        usersIdDislikes: [],\r\n        liked: false,\r\n        disliked: false,\r\n        openedInput: false,\r\n        answered: [{\r\n          user: 'username',\r\n          date: formattedToday,\r\n          comment: 'Ответ на Хороший тест!',\r\n          usersIdLikes: [1, 2],\r\n          usersIdDislikes: [],\r\n          liked: false,\r\n          disliked: false,\r\n          answered: []\r\n        }]\r\n      },\r\n      {\r\n        user: 'some user',\r\n        date: formattedToday,\r\n        stars: 5,\r\n        comment: 'Отличный тест!',\r\n        usersIdLikes: [1, 2],\r\n        usersIdDislikes: [],\r\n        liked: false,\r\n        disliked: false,\r\n        openedInput: false,\r\n        answered: []\r\n      },\r\n    ]);\r\n    return {\r\n      comments,\r\n      putLikeDis,\r\n      starSave,\r\n      leaveAComment,\r\n      inputComment,\r\n      answerComment,\r\n      commentAnswer,\r\n      openInput,\r\n      store\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";AAsGA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAI,IAAIC,IAAI,EAAE;IACxB,MAAMC,IAAG,GAAIF,KAAK,CAACG,WAAW,EAAE;IAChC,IAAIC,EAAC,GAAIJ,KAAK,CAACK,QAAQ,EAAC,GAAI,CAAC;IAC7B,IAAIC,EAAC,GAAIN,KAAK,CAACO,OAAO,EAAE;IACxB,IAAID,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAC1B,IAAIF,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAC1B,MAAMI,cAAa,GAAIF,EAAC,GAAI,GAAE,GAAIF,EAAC,GAAI,GAAE,GAAIF,IAAI;IAEjD,IAAIO,UAAS,GAAIb,GAAG,CAAC,CAAC,CAAC;IACvB,IAAIc,YAAW,GAAId,GAAG,CAAC,EAAE,CAAC;IAC1B,IAAIe,aAAY,GAAIf,GAAG,CAAC,EAAE,CAAC;IAE3B,IAAIgB,QAAO,GAAI,SAAAA,CAAUC,IAAI,EAAE;MAC7BJ,UAAU,CAACK,KAAI,GAAID,IAAI;IACzB;IAEA,IAAIE,UAAS,GAAI,SAAAA,CAAUC,OAAO,EAAEC,SAAS,EAAEC,aAAa,EAAEC,IAAI,EAAEC,OAAO,EAAEC,MAAM,EAAE;MACnF,IAAIxB,KAAK,CAACyB,OAAO,CAACC,OAAO,EAAE;QACzB,IAAIN,SAAS,CAACO,OAAO,CAACH,MAAM,KAAK,CAAC,KAAKH,aAAa,CAACM,OAAO,CAACH,MAAM,KAAK,CAAC,CA