{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"Comments\",\n  setup() {\n    const today = new Date();\n    const yyyy = today.getFullYear();\n    let mm = today.getMonth() + 1; // Months start at 0!\n    let dd = today.getDate();\n    if (dd < 10) dd = '0' + dd;\n    if (mm < 10) mm = '0' + mm;\n    const formattedToday = dd + '/' + mm + '/' + yyyy;\n    let active = ref(false);\n    let putLikeDis = function (mainInter, oppositeInter, userId) {\n      if (mainInter.indexOf(userId) == -1 && oppositeInter.indexOf(userId) == -1) {\n        mainInter.push(userId);\n      } else if (mainInter.indexOf(userId) == -1 && oppositeInter.indexOf(userId) != -1) {\n        mainInter.push(userId);\n        oppositeInter.splice(oppositeInter.findIndex(id => id === userId), 1);\n        active.value = true;\n      } else if (mainInter.indexOf(userId) != -1 && oppositeInter.indexOf(userId) == -1) {\n        mainInter.splice(mainInter.findIndex(id => id === userId), 1);\n        active.value = false;\n      }\n    };\n    let comments = ref([{\n      user: 'username',\n      date: formattedToday,\n      stars: 4,\n      comment: 'Хороший тест!',\n      usersIdLikes: [1, 2],\n      usersIdDislikes: []\n    }]);\n    return {\n      comments,\n      putLikeDis,\n      active\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","today","Date","yyyy","getFullYear","mm","getMonth","dd","getDate","formattedToday","active","putLikeDis","mainInter","oppositeInter","userId","indexOf","push","splice","findIndex","id","value","comments","user","date","stars","comment","usersIdLikes","usersIdDislikes"],"sources":["E:\\Vue\\career_guidance\\src\\components\\Comments.vue"],"sourcesContent":["<template>\r\n<div class=\"comments\">\r\n  <h2 class=\"comments__title\">Поделитесь со своми впечатлениями (1)</h2>\r\n  <div class=\"comments__leaveAComment\">\r\n    <span class=\"comments__stars\">\r\n      <i>★</i><i>★</i><i>★</i><i>★</i><i>★</i>\r\n    </span>\r\n    <div class=\"comments__input\">\r\n      <input class=\"comments__input-input\" type=\"text\" placeholder=\"Напишите комментарий\">\r\n      <img class=\"comments__input-img\" src=\"../images/png/ArrowToRightBlue.png\" alt=\"arrow\">\r\n    </div>\r\n  </div><!-- ./comments__leaveAComment -->\r\n  <div class=\"comments__list\">\r\n    <div class=\"comments__item\" v-for=\"comment in comments\" :key=\"comment.date\">\r\n      <div class=\"comments__user\">\r\n        <div class=\"comments__user-name\">{{ comment.user }}</div>\r\n        <div class=\"comments__user-date\">{{ comment.date }}</div>\r\n        <div class=\"comments__user-stars\">\r\n          <img src=\"../images/png/Star-black.png\" alt=\"star\" v-for=\"item in comment.stars\">\r\n        </div>\r\n      </div><!-- ./comments__user -->\r\n      <div class=\"comments__item-text\">{{ comment.comment }}</div>\r\n      <div class=\"comments__item-interaction\">\r\n        <div class=\"comments__item-answer\">Ответить</div>\r\n        <div class=\"comments__item-response\">\r\n          <div class=\"comments__item-likes\">\r\n            <img\r\n                @click=\"putLikeDis(comment.usersIdLikes, comment.usersIdDislikes, 3)\"\r\n                class=\"comments__item-interactionImage\"\r\n                :class=\"active ? 'active' : ''\"\r\n                src=\"../images/png/mdi_like.png\"\r\n                alt=\"likes\">\r\n            <span>{{ comment.usersIdLikes.length }}</span>\r\n          </div>\r\n          <div class=\"comments__item-dislikes\">\r\n            <img\r\n                @click=\"putLikeDis(comment.usersIdDislikes, comment.usersIdLikes, 3)\"\r\n                class=\"comments__item-interactionImage\"\r\n                :class=\"active ? 'active' : ''\"\r\n                src=\"../images/png/mdi_dislike.png\"\r\n                alt=\"dis\">\r\n            <span>{{ comment.usersIdDislikes.length }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div><!-- ./comments__item -->\r\n  </div><!-- ./comments__list -->\r\n</div><!-- ./comments -->\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\n\r\nexport default {\r\n  name: \"Comments\",\r\n  setup () {\r\n    const today = new Date();\r\n    const yyyy = today.getFullYear();\r\n    let mm = today.getMonth() + 1; // Months start at 0!\r\n    let dd = today.getDate();\r\n\r\n    if (dd < 10) dd = '0' + dd;\r\n    if (mm < 10) mm = '0' + mm;\r\n\r\n    const formattedToday = dd + '/' + mm + '/' + yyyy;\r\n    let active = ref(false)\r\n\r\n    let putLikeDis = function (mainInter, oppositeInter, userId) {\r\n      if (mainInter.indexOf(userId) == -1 && oppositeInter.indexOf(userId) == -1) {\r\n        mainInter.push(userId);\r\n      }\r\n      else if (mainInter.indexOf(userId) == -1 && oppositeInter.indexOf(userId) != -1) {\r\n        mainInter.push(userId);\r\n        oppositeInter.splice(oppositeInter.findIndex(id => id === userId), 1);\r\n        active.value = true;\r\n      }\r\n      else if (mainInter.indexOf(userId) != -1 && oppositeInter.indexOf(userId) == -1) {\r\n        mainInter.splice(mainInter.findIndex(id => id === userId), 1)\r\n        active.value = false;\r\n      }\r\n    };\r\n\r\n    let comments = ref([\r\n      {\r\n        user: 'username',\r\n        date: formattedToday,\r\n        stars: 4,\r\n        comment: 'Хороший тест!',\r\n        usersIdLikes: [1, 2],\r\n        usersIdDislikes: [],\r\n      },\r\n    ]);\r\n    return {\r\n      comments,\r\n      putLikeDis,\r\n      active\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAmDA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAI,IAAIC,IAAI,EAAE;IACxB,MAAMC,IAAG,GAAIF,KAAK,CAACG,WAAW,EAAE;IAChC,IAAIC,EAAC,GAAIJ,KAAK,CAACK,QAAQ,EAAC,GAAI,CAAC,EAAE;IAC/B,IAAIC,EAAC,GAAIN,KAAK,CAACO,OAAO,EAAE;IAExB,IAAID,EAAC,GAAI,EAAE,EAAEA,EAAC,GAA