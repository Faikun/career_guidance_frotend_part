href=\"#\">История</a>\r\n              <a href=\"#\" @click=\"exitAcc\">Выход</a>\r\n            </div>\r\n          </div>\r\n        </div><!-- ./nav__right -->\r\n      </div><!-- ./nav__wrapper -->\r\n      <transition name=\"fadeHeight\">\r\n        <div class=\"search\" v-if=\"searchActive\">\r\n          <input class=\"search__input\" type=\"text\" v-model.trim=\"search\" placeholder=\"Введите тест, профессию или вуз\">\r\n          <img class=\"search__icon\" src=\"../images/png/Search.png\" alt=\"icon\">\r\n        </div><!-- ./search -->\r\n      </transition>\r\n    </div><!-- ./container -->\r\n  </div><!-- ./nav -->\r\n\r\n  <div class=\"container\">\r\n    <notification\r\n        v-if=\"notification.openedNotification\"\r\n        :title=\"notification.title\"\r\n        :text=\"notification.text\"\r\n        :mode=\"notification.mode\"\r\n    ></notification>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {computed, ref} from \"vue\";\r\nimport store from \"@/store\";\r\nimport Notification from \"@/components/notification.vue\";\r\n\r\nexport default {\r\n  name: \"TheNavbar\",\r\n  components: { Notification },\r\n  setup() {\r\n    let search = ref('');\r\n    let account = ref('');\r\n    let searchActive = ref(false);\r\n    let toggle = ref(false);\r\n    const storeGetters = computed(() => store.getters);\r\n    let notification = store.getters.getNotificationInfo;\r\n\r\n    let exitAcc = function () {\r\n      store.commit('setAuth')\r\n      account.value = !account.value;\r\n      store.commit('setNotificationInfo', { titleValue: 'Успех', text: 'Вы успешно вышли со своего аккаунта', mode: 'success'} )\r\n    }\r\n\r\n    let showAccBlock = function () {\r\n      if (store.getters.getAuth) {\r\n        account.value = !account.value;\r\n      }\r\n      else {\r\n        store.commit('toggleOpenedAuth');\r\n        document.body.style.overflow = \"hidden\";\r\n        document.documentElement.scrollTop = 0;\r\n      }\r\n    }\r\n    return {\r\n      search,\r\n      searchActive,\r\n      toggle,\r\n      account,\r\n      showAccBlock,\r\n      store, storeGetters,\r\n      exitAcc,\r\n      notification\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .fadeHeight-enter-active,\r\n  .fadeHeight-leave-active {\r\n    transition: all 0.2s;\r\n    max-height: 230px;\r\n  }\r\n  .fadeHeight-enter,\r\n  .fadeHeight-leave-to\r\n  {\r\n    opacity: 0;\r\n    max-height: 0px;\r\n  }\r\n\r\n</style>\r\n"],"mappings":"AA0EA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAEF;EAAa,CAAC;EAC5BG,KAAKA,CAAA,EAAG;IACN,IAAIC,MAAK,GAAIN,GAAG,CAAC,EAAE,CAAC;IACpB,IAAIO,OAAM,GAAIP,GAAG,CAAC,EAAE,CAAC;IACrB,IAAIQ,YAAW,GAAIR,GAAG,CAAC,KAAK,CAAC;IAC7B,IAAIS,MAAK,GAAIT,GAAG,CAAC,KAAK,CAAC;IACvB,MAAMU,YAAW,GAAIX,QAAQ,CAAC,MAAME,KAAK,CAACU,OAAO,CAAC;IAClD,IAAIC,YAAW,GAAIX,KAAK,CAACU,OAAO,CAACE,mBAAmB;IAEpD,IAAIC,OAAM,GAAI,SAAAA,CAAA,EAAY;MACxBb,KAAK,CAACc,MAAM,CAAC,SAAS;MACtBR,OAAO,CAACS,KAAI,GAAI,CAACT,OAAO,CAACS,KAAK;MAC9Bf,KAAK,CAACc,MAAM,CAAC,qBAAqB,EAAE;QAAEE,UAAU,EAAE,OAAO;QAAEC,IAAI,EAAE,qCAAqC;QAAEC,IAAI,EAAE;MAAS,EAAE;IAC3H;IAEA,IAAIC,YAAW,GAAI,SAAAA,CAAA,EAAY;MAC7B,IAAInB,KAAK,CAACU,OAAO,CAACU,OAAO,EAAE;QACzBd,OAAO,CAACS,KAAI,GAAI,CAACT,OAAO,CAACS,KAAK;MAChC,OACK;QACHf,KAAK,CAACc,MAAM,CAAC,kBAAkB,CAAC;QAChCO,QAAQ,CAACC,IAAI,CAACC,KAAK,CAACC,QAAO,GAAI,QAAQ;QACvCH,QAAQ,CAACI,eAAe,CAACC,SAAQ,GAAI,CAAC;MACxC;IACF;IACA,OAAO;MACLrB,MAAM;MACNE,YAAY;MACZC,MAAM;MACNF,OAAO;MACPa,YAAY;MACZnB,KAAK;MAAES,YAAY;MACnBI,OAAO;MACPF;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}