cts')\r\n    let subjects = reactive({\r\n      math: false,\r\n      physics: false,\r\n      russian: false,\r\n      kazakh: false,\r\n      english: false,\r\n      criticalThinking: false,\r\n      IELTS: false,\r\n      biology: false,\r\n      chemistry: false,\r\n      worldHistory: false,\r\n      geography: false,\r\n      audioVisual: false\r\n    });\r\n    let hotel = ref(false),\r\n        military = ref(false);\r\n\r\n    let findCity = computed(() => universities.filter((el) => el.location === props.city));\r\n    let findSubVal = computed(() => Object.values(subjects));\r\n\r\n    const getImgUrl = function (img) {\r\n      if(img.includes('http'))\r\n        return img\r\n\r\n      return require('@/images/' + img);\r\n    }\r\n    let searchEdu = function () {\r\n      let city = findCity.value;\r\n      console.log(city)\r\n      const subjectValues = findSubVal.value;\r\n      console.log(subjectValues)\r\n      console.log(edu)\r\n\r\n      if(edu.value.length > 0)\r\n        city = city.filter((el) => el.name.toUpperCase().match(edu.value.toUpperCase()));\r\n\r\n      if(status.value !== 'Статус: Любой')\r\n        city = city.filter((el) => el.status === status.value)\r\n\r\n      if(area.value !== 'Область обучения')\r\n        city = city.filter((el) => el.tags.includes(area.value))\r\n\r\n      if(military.value)\r\n        city = city.filter((el) => el.military === true)\r\n\r\n      if(hotel.value)\r\n        city = city.filter((el) => el.hotel === true)\r\n\r\n      if(subjectValues.filter((el) => el === true).toString()) {\r\n        if(subjects.math)\r\n          city = city.filter((el) => el.subjects.includes('математика'))\r\n\r\n        if(subjects.chemistry)\r\n          city = city.filter((el) => el.subjects.includes('химия'))\r\n\r\n        if(subjects.physics)\r\n          city = city.filter((el) => el.subjects.includes('физика'))\r\n\r\n        if(subjects.worldHistory)\r\n          city = city.filter((el) => el.subjects.includes('всемирная история'))\r\n\r\n        if(subjects.english)\r\n          city = city.filter((el) => el.subjects.includes('английский'))\r\n\r\n        if(subjects.geography)\r\n          city = city.filter((el) => el.subjects.includes('география'))\r\n\r\n        if(subjects.audioVisual)\r\n          city = city.filter((el) => el.subjects.includes('Аудиовизуальное искусство'))\r\n      }\r\n\r\n      searchDone.value = true;\r\n      return city\r\n    }\r\n\r\n\r\n    return {\r\n      dropdownActive,\r\n      searchEdu, edu,\r\n      searchDone,\r\n      status, area, subjects,\r\n      hotel, military,\r\n      getImgUrl\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AA2GA,SAAQA,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,EAAEC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACtD,SAAQC,QAAQ,QAAO,YAAY;AACnC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE,CAAC,MAAM,CAAC;EACf,MAAMC,KAAKA,CAACD,KAAK,EAAE;IACjB,MAAME,KAAI,GAAIV,QAAQ,EAAC;IAEvBE,SAAS,CAAC,MAAMS,SAAS,EAAE,CAAC;IAC5BT,SAAS,CAAC,MAAMU,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC,CAAC;IAEzC,MAAMH,KAAK,CAACI,QAAQ,CAAC,oBAAoB;IACzC,IAAIC,YAAW,GAAIL,KAAK,CAACM,OAAO,CAACC,aAAa;IAE9C,IAAIC,cAAa,GAAId,GAAG,CAAC,KAAK,CAAC;IAC/B,IAAIe,UAAS,GAAIf,GAAG,CAAC,KAAK,CAAC;IAE3B,IAAIgB,KAAI,GAAIf,QAAQ,EAAE;IAEtB,IAAIgB,GAAE,GAAIjB,GAAG,CAAC,EAAE,CAAC;IACjB,IAAIkB,MAAK,GAAIlB,GAAG,CAAC,eAAe,CAAC;IACjC,IAAImB,IAAG,GAAInB,GAAG,CAAC,kBAAkB,CAAC;IAClC,IAAIoB,GAAE,GAAIlB,KAAK,CAACmB,GAAG,CAAC,UAAU;IAC9B,IAAIC,QAAO,GAAIvB,QAAQ,CAAC;MACtBwB,IAAI,EAAE,KAAK;MACXC,OAAO,EAAE,KAAK;MACdC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE,KAAK;MACbC,OAAO,EAAE,KAAK;MACdC,gBAAgB,EAAE,KAAK;MACvBC,KAAK,EAAE,KAAK;MACZC,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE,KAAK;MACnBC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAE;IACf,CAAC,CAAC;IACF,IAAIC,KAAI,GAAInC,GAAG,CAAC,KAAK,CAAC;MAClBoC,QAAO,GAAIpC,GAAG,CAAC,KAAK,CAAC;IAEzB,IAAIqC,QAAO,GAAIxC,QAAQ,CAAC,MAAMc,YAAY,CAAC2B,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACC,QAAO,KAAMpC,KAAK,CAACqC,IAAI,CAAC,CA