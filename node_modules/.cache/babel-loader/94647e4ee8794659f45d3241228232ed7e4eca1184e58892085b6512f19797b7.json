ts -->\r\n</template>\r\n\r\n<script>\r\nimport {useStore} from \"vuex\";\r\n\r\nexport default {\r\n  name: \"EduTemplate\",\r\n  props: ['searchEdu', 'searchDone'],\r\n  emits: ['university'],\r\n  async setup() {\r\n    const store = useStore()\r\n    await store.dispatch('setAllUniversities')\r\n    let university = store.getters.getUniversity;\r\n\r\n    let sendDataToParent = function () {\r\n      this.$emit('university', university)\r\n    }\r\n\r\n    return {\r\n      university\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAoCA,SAAQA,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE,CAAC,WAAW,EAAE,YAAY,CAAC;EAClCC,KAAK,EAAE,CAAC,YAAY,CAAC;EACrB,MAAMC,KAAKA,CAAA,EAAG;IACZ,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMK,KAAK,CAACC,QAAQ,CAAC,oBAAoB;IACzC,IAAIC,UAAS,GAAIF,KAAK,CAACG,OAAO,CAACC,aAAa;IAE5C,IAAIC,gBAAe,GAAI,SAAAA,CAAA,EAAY;MACjC,IAAI,CAACC,KAAK,CAAC,YAAY,EAAEJ,UAAU;IACrC;IAEA,OAAO;MACLA;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}