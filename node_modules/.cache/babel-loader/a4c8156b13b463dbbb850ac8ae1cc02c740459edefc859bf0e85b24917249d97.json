onents"],"sources":["E:\\Vue\\career_guidance\\src\\components\\SliderSwiper.vue"],"sourcesContent":["<template>\r\n  <swiper\r\n    :modules=\"modules\"\r\n    :loop=\"true\"\r\n    :pagination=\"{ clickable: true }\"\r\n    :autoplay=\"{\r\n        delay: 7000,\r\n        disableOnInteraction: false,\r\n        pauseOnMouseEnter: true\r\n      }\"\r\n    :breakpoints=\"{\r\n        320: {\r\n         slidesPerView: 1,\r\n         spaceBetween: 10\r\n      },\r\n        480: {\r\n         slidesPerView: 2,\r\n         spaceBetween: 15\r\n      },\r\n        640: {\r\n         slidesPerView: 3,\r\n         spaceBetween: 20\r\n        },\r\n        1000: {\r\n         slidesPerView: 4,\r\n         spaceBetween: 25\r\n        }\r\n      }\"\r\n  >\r\n    <swiper-slide v-for=\"profession in prof\" :key=\"profession\" @click=\"goToProf(profession.name)\">\r\n      <div class=\"result_slide\">\r\n        <div class=\"result__img\">\r\n          <img :src=\"getImgUrl(profession.image)\" alt=\"food\">\r\n        </div>\r\n        <span class=\"result__name\">{{ profession.name }}</span>\r\n        <span class=\"result__description\">{{ truncateText(profession.description, 125) }}</span>\r\n        <span class=\"result__detail\">Подробнее</span>\r\n      </div>\r\n    </swiper-slide>\r\n\r\n    <!-- If we need pagination -->\r\n    <div class=\"swiper-pagination\"></div>\r\n\r\n  </swiper>\r\n</template>\r\n\r\n<script>\r\nimport 'swiper/css';\r\nimport 'swiper/css/navigation';\r\nimport 'swiper/css/pagination';\r\n\r\nimport {Autoplay, Pagination, A11y} from 'swiper'\r\nimport {Swiper, SwiperSlide } from 'swiper/vue'\r\nimport {useRouter} from \"vue-router\";\r\nimport {useStore} from \"vuex\";\r\nimport {computed, onMounted} from \"vue\";\r\n\r\nexport default {\r\n  name: 'SliderSwiper',\r\n  props: ['array', 'res'],\r\n  setup (props) {\r\n    onMounted(async () => store.dispatch('initialProfessionsList'))\r\n    const router = useRouter()\r\n    const store = useStore()\r\n\r\n    let prof = computed(() => store.getters.getProfessionsArray.filter((el) => props.array.includes(el.name.toLowerCase())));\r\n\r\n    const goToProf = function (name) {\r\n      const prof = store.getters.getProfessionsArray.filter((el) => el.name.toLowerCase() === name.toLowerCase())[0];\r\n      router.push({ name: 'Profession', params: { name: prof.category, prof: prof.name }})\r\n    }\r\n\r\n    const getImgUrl = function (img) {\r\n      if(img.includes('http'))\r\n        return img\r\n\r\n      return require('@/images/Prof/Certain/' + img);\r\n    }\r\n\r\n    const truncateText = function (text, maxLength) {\r\n      if (text.length > maxLength) {\r\n        return text.substring(0, maxLength - 3) + '...';\r\n      } else {\r\n        return text;\r\n      }\r\n    }\r\n\r\n    return {\r\n      modules: [Autoplay, Pagination, A11y],\r\n      goToProf,\r\n      prof,\r\n      getImgUrl,\r\n      truncateText\r\n    }\r\n  },\r\n  components: {\r\n    Swiper, SwiperSlide\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .swiper, .swiper-container {\r\n    min-height: 500px;\r\n  }\r\n</style>\r\n"],"mappings":";AA+CA,OAAO,YAAY;AACnB,OAAO,uBAAuB;AAC9B,OAAO,uBAAuB;AAE9B,SAAQA,QAAQ,EAAEC,UAAU,EAAEC,IAAI,QAAO,QAAO;AAChD,SAAQC,MAAM,EAAEC,WAAU,QAAS,YAAW;AAC9C,SAAQC,SAAS,QAAO,YAAY;AACpC,SAAQC,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,EAAEC,SAAS,QAAO,KAAK;AAEvC,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAK,EAAE,CAAC,OAAO,EAAE,KAAK,CAAC;EACvBC,KAAIA,CAAGD,KAAK,EAAE;IACZF,SAAS,CAAC,YAAYI,KAAK,CAACC,QAAQ,CAAC,wBAAwB,CAAC;IAC9D,MAAMC,MAAK,GAAIT,SAAS,EAAC;IACzB,MAAMO,KAAI,GAAIN,QAAQ,EAAC;IAEvB,IAAIS,IAAG,GAAIR,QAAQ,CAAC,MAAMK,KAAK,CAACI,OAAO,CAACC,mBAAmB,CAACC,MAAM,CAAEC,EAAE,IAAKT,KAAK,CAACU,KAAK,CAACC,QAAQ,CAACF,EAAE,CAACV,IAAI,CAACa,WAAW,EAAE,CAAC,CAAC,CAAC;IAExH,MAAMC,QAAO,GAAI,SAAAA,CAAUd,IAAI,EAAE;MAC/B,MAAMM,IAAG,GAAIH,KAAK,CAACI,OAAO,CAACC,mBAAmB,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACV,IAAI,CAACa,WAAW,EAAC,KAAMb,IAAI,CAACa,WAAW,EAAE,CAAC,CAAC,CAAC,CAAC;MAC9GR,MAAM,CAACU,IAA