{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"Comments\",\n  setup() {\n    const today = new Date();\n    const yyyy = today.getFullYear();\n    let mm = today.getMonth() + 1; // Months start at 0!\n    let dd = today.getDate();\n    if (dd < 10) dd = '0' + dd;\n    if (mm < 10) mm = '0' + mm;\n    const formattedToday = dd + '/' + mm + '/' + yyyy;\n    let putLikeDis = function (interaction, userId) {\n      if (interaction.indexOf(userId) == -1 && interaction.indexOf(userId) == -1) {\n        interaction.push(userId);\n      } else if (comment.usersIdLikes.indexOf(userId) == -1 && interaction.indexOf(userId) == 1) {\n        interaction.push(userId);\n        interaction.splice(comment.usersIdDislikes.findIndex(id => id === userId), 1);\n      } else {\n        interaction.splice(comment.usersIdLikes.findIndex(id => id === userId), 1);\n      }\n    };\n    let comments = ref([{\n      user: 'username',\n      date: formattedToday,\n      stars: 4,\n      comment: 'Хороший тест!',\n      usersIdLikes: [1, 2],\n      usersIdDislikes: []\n    }]);\n    return {\n      comments,\n      putLikeDis\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","today","Date","yyyy","getFullYear","mm","getMonth","dd","getDate","formattedToday","putLikeDis","interaction","userId","indexOf","push","comment","usersIdLikes","splice","usersIdDislikes","findIndex","id","comments","user","date","stars"],"sources":["E:\\Vue\\career_guidance\\src\\components\\Comments.vue"],"sourcesContent":["<template>\r\n<div class=\"comments\">\r\n  <h2 class=\"comments__title\">Поделитесь со своми впечатлениями (1)</h2>\r\n  <div class=\"comments__leaveAComment\">\r\n    <span class=\"comments__stars\">\r\n      <i>★</i><i>★</i><i>★</i><i>★</i><i>★</i>\r\n    </span>\r\n    <div class=\"comments__input\">\r\n      <input class=\"comments__input-input\" type=\"text\" placeholder=\"Напишите комментарий\">\r\n      <img class=\"comments__input-img\" src=\"../images/png/ArrowToRightBlue.png\" alt=\"arrow\">\r\n    </div>\r\n  </div><!-- ./comments__leaveAComment -->\r\n  <div class=\"comments__list\">\r\n    <div class=\"comments__item\" v-for=\"comment in comments\" :key=\"comment.date\">\r\n      <div class=\"comments__user\">\r\n        <div class=\"comments__user-name\">{{ comment.user }}</div>\r\n        <div class=\"comments__user-date\">{{ comment.date }}</div>\r\n        <div class=\"comments__user-stars\">\r\n          <img src=\"../images/png/Star-black.png\" alt=\"star\" v-for=\"item in comment.stars\">\r\n        </div>\r\n      </div><!-- ./comments__user -->\r\n      <div class=\"comments__item-text\">{{ comment.comment }}</div>\r\n      <div class=\"comments__item-interaction\">\r\n        <div class=\"comments__item-answer\">Ответить</div>\r\n        <div class=\"comments__item-response\">\r\n          <div class=\"comments__item-likes\">\r\n            <img @click=\"putLikeDis(comment.usersIdLikes, 3)\" class=\"comments__item-interactionImage\" src=\"../images/png/mdi_like.png\" alt=\"likes\">\r\n            <span>{{ comment.usersIdLikes.length }}</span>\r\n          </div>\r\n          <div class=\"comments__item-dislikes\">\r\n            <img  @click=\"putLikeDis(comment.usersIdDislikes, 3)\" class=\"comments__item-interactionImage\" src=\"../images/png/mdi_dislike.png\" alt=\"dis\">\r\n            <span>{{ comment.usersIdDislikes.length }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div><!-- ./comments__item -->\r\n  </div><!-- ./comments__list -->\r\n</div><!-- ./comments -->\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\";\r\n\r\nexport default {\r\n  name: \"Comments\",\r\n  setup () {\r\n    const today = new Date();\r\n    const yyyy = today.getFullYear();\r\n    let mm = today.getMonth() + 1; // Months start at 0!\r\n    let dd = today.getDate();\r\n\r\n    if (dd < 10) dd = '0' + dd;\r\n    if (mm < 10) mm = '0' + mm;\r\n\r\n    const formattedToday = dd + '/' + mm + '/' + yyyy;\r\n\r\n    let putLikeDis = function (interaction, userId) {\r\n      if (interaction.indexOf(userId) == -1 && interaction.indexOf(userId) == -1) {\r\n        interaction.push(userId);\r\n      }\r\n      else if (comment.usersIdLikes.indexOf(userId) == -1 && interaction.indexOf(userId) == 1) {\r\n        interaction.push(userId);\r\n        interaction.splice(comment.usersIdDislikes.findIndex(id => id === userId), 1)\r\n      }\r\n      else {\r\n        interaction.splice(comment.usersIdLikes.findIndex(id => id === userId), 1)\r\n      }\r\n    };\r\n\r\n    let comments = ref([\r\n      {\r\n        user: 'username',\r\n        date: formattedToday,\r\n        stars: 4,\r\n        comment: 'Хороший тест!',\r\n        usersIdLikes: [1, 2],\r\n        usersIdDislikes: [],\r\n      },\r\n    ]);\r\n    return {\r\n      comments,\r\n      putLikeDis\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAyCA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAI,IAAIC,IAAI,EAAE;IACxB,MAAMC,IAAG,GAAIF,KAAK,CAACG,WAAW,EAAE;IAChC,IAAIC,EAAC,GAAIJ,KAAK,CAACK,QAAQ,EAAC,GAAI,CAAC,EAAE;IAC/B,IAAIC,EAAC,GAAIN,KAAK,CAACO,OAAO,EAAE;IAExB,IAAID,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAC1B,IAAIF,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAE1B,MAAMI,cAAa,GAAIF,EAAC,GAAI,GAAE,GAAIF,EAAC,GAAI,GAAE,GAAIF,IAAI;IAEjD,IAAIO,UAAS,GAAI,SAAAA,CAAUC,WAAW,EAAEC,MAAM,EAAE;MAC9C,IAAID,WAAW,CAACE,OAAO,CAACD,MAAM,KAAK,CAAC,KAAKD,WAAW,CAACE,OAAO,CAACD,MAAM,KAAK,CAAC,CAAC,EAAE;QAC1ED,WAAW,CAACG,IAAI,CAACF,MAAM,CAAC;MAC1B,OACK,IAAIG,OAAO,CAACC,YAAY,CAACH,OAAO,CAACD,MAAM,KAAK,CAAC,KAAKD,WAAW,CAACE,OAAO,CAACD,MAAM,KAAK,CAAC,EAAE;QACvFD,WAAW,CAACG,IAAI,CAACF,MAAM,CAAC;QACxBD,WAAW,CAACM,MAAM,CAACF,OAAO,CAACG,eAAe,CAACC,SAAS,CAACC,EAAC,IAAKA,EAAC,KAAMR,MAAM,CAAC,EAAE,CAAC;MAC9E,OACK;QACHD,WAAW,CAACM,MAAM,CAACF,OAAO,CAACC,YAAY,CAACG,SAAS,CA