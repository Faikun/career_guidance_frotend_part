       photo = ref(''),\r\n        medCertificate = ref(''),\r\n        fluorography = ref(''),\r\n        healthPassport = ref(''),\r\n        satisfaction = ref('');\r\n    const validation = function () {\r\n      if(IIN.value.length !== 8) {\r\n        return false\r\n      }\r\n      if(!application.value.length) {\r\n        return false\r\n      }\r\n      if(!education.value.length) {\r\n        return false\r\n      }\r\n      if(!photo.value.length) {\r\n        return false\r\n      }\r\n      if(!medCertificate.value.length) {\r\n        return false\r\n      }\r\n      if(!fluorography.value.length) {\r\n        return false\r\n      }\r\n      if(!healthPassport.value.length) {\r\n        return false\r\n      }\r\n      if(!satisfaction.value.length) {\r\n        return false\r\n      }\r\n      return true\r\n    }\r\n    let makeARequest = function () {\r\n      if(validation()) {\r\n        console.log('everything is okay')\r\n      }\r\n    }\r\n    return {\r\n      makeARequest,\r\n      IIN, application, education, photo,\r\n      medCertificate, fluorography, healthPassport, satisfaction\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AA6CA,SAAQA,GAAG,QAAO,KAAK;AAEvB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,KAAKA,CAAA,EAAG;IACN,IAAIC,GAAE,GAAIJ,GAAG,CAAC,EAAE,CAAC;MACbK,WAAU,GAAIL,GAAG,CAAC,EAAE,CAAC;MACrBM,SAAQ,GAAIN,GAAG,CAAC,EAAE,CAAC;MACnBO,KAAI,GAAIP,GAAG,CAAC,EAAE,CAAC;MACfQ,cAAa,GAAIR,GAAG,CAAC,EAAE,CAAC;MACxBS,YAAW,GAAIT,GAAG,CAAC,EAAE,CAAC;MACtBU,cAAa,GAAIV,GAAG,CAAC,EAAE,CAAC;MACxBW,YAAW,GAAIX,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMY,UAAS,GAAI,SAAAA,CAAA,EAAY;MAC7B,IAAGR,GAAG,CAACS,KAAK,CAACC,MAAK,KAAM,CAAC,EAAE;QACzB,OAAO,KAAI;MACb;MACA,IAAG,CAACT,WAAW,CAACQ,KAAK,CAACC,MAAM,EAAE;QAC5B,OAAO,KAAI;MACb;MACA,IAAG,CAACR,SAAS,CAACO,KAAK,CAACC,MAAM,EAAE;QAC1B,OAAO,KAAI;MACb;MACA,IAAG,CAACP,KAAK,CAACM,KAAK,CAACC,MAAM,EAAE;QACtB,OAAO,KAAI;MACb;MACA,IAAG,CAACN,cAAc,CAACK,KAAK,CAACC,MAAM,