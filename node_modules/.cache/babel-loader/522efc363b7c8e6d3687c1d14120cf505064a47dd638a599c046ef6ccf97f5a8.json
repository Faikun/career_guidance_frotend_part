{"ast":null,"code":"import { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\nimport { computed, onMounted } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"Profession\",\n  setup() {\n    let store = useStore();\n    onMounted(() => {\n      store.dispatch('initialProfessionsList');\n      store.dispatch('setAllUniversities');\n    });\n    let route = useRoute();\n    const profArray = computed(() => store.getters.getProfessionsArray.filter(el => el.name === route.params.prof));\n    let university = computed(() => store.getters.getUniversity);\n    const getImgUrl = function (img) {\n      if (img.includes('http')) return img;\n      return require('@/images/Prof/Certain/' + img);\n    };\n    return {\n      store,\n      prof: profArray,\n      university,\n      getImgUrl\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useStore","computed","onMounted","axios","name","setup","store","dispatch","route","profArray","getters","getProfessionsArray","filter","el","params","prof","university","getUniversity","getImgUrl","img","includes","require"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Profession.vue"],"sourcesContent":["<template>\r\n  <div class=\"profession\" v-if=\"prof\">\r\n    <h1 class=\"profession__title\">{{ this.$route.params.prof }}</h1>\r\n    <div class=\"profession__article\">\r\n      <div class=\"profession__author\">Admin</div>\r\n      <div class=\"profession__date\">11 марта 2023</div>\r\n    </div> <!-- ./profession__article -->\r\n    <div class=\"profession__image\">\r\n      <img :src=\"getImgUrl(prof[0].image)\" alt=\"image\">\r\n    </div>\r\n    <div class=\"profession__contents\">\r\n      <h2 class=\"profession__contents-title\">Содержание</h2>\r\n      <ul class=\"profession__contents-list\">\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#description\">Описание</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#whatToDo\">Что нужно делать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#know\">Что нужно знать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#money\">Сколько можно заработать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#study\">Где можно обучиться</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"profession__info\" v-for=\"array in prof\">\r\n      <h2 class=\"profession__info-title\" id=\"description\">Описание</h2>\r\n      <div class=\"profession__info-text\">{{ array.description }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"whatToDo\">Что нужно делать</h2>\r\n      <div class=\"profession__info-text\">{{ array.whatMakes }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"know\">Что нужно знать</h2>\r\n      <div class=\"profession__info-text\">{{ array.whatShouldKnow }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"money\">Сколько можно заработать</h2>\r\n      <div class=\"profession__info-text\">{{ array.money }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"study\">Где можно обучиться</h2>\r\n      <div class=\"profession__info-text\">\r\n        <ul v-if=\"university.filter((el) => el.tags.some(tag => tag.name.includes(array.category))).map((el) => el.name).length > 0\">\r\n          <li v-for=\"item in university.filter((el) => el.tags.some(tag => tag.name.includes(array.category))).map((el) => el.name)\">\r\n            <router-link :to=\"'/university/' + item\">{{ item }}</router-link>\r\n          </li>\r\n        </ul>\r\n        <span v-else>Список пуст</span>\r\n      </div>\r\n    </div><!-- ./profession__info -->\r\n  </div><!-- ./profession -->\r\n</template>\r\n\r\n<script>\r\nimport {useRoute} from \"vue-router\";\r\nimport {useStore} from \"vuex\";\r\nimport {computed, onMounted} from \"vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Profession\",\r\n  setup() {\r\n    let store = useStore();\r\n\r\n    onMounted( () => {\r\n      store.dispatch('initialProfessionsList')\r\n      store.dispatch('setAllUniversities')\r\n    })\r\n\r\n    let route = useRoute();\r\n    const profArray = computed(() => store.getters.getProfessionsArray.filter((el) => el.name === route.params.prof))\r\n\r\n    let university = computed(() => store.getters.getUniversity);\r\n    const getImgUrl = function (img) {\r\n      if(img.includes('http'))\r\n        return img\r\n\r\n      return require('@/images/Prof/Certain/' + img);\r\n    }\r\n    return {\r\n      store,\r\n      prof: profArray,\r\n      university,\r\n      getImgUrl\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAqDA,SAAQA,QAAQ,QAAO,YAAY;AACnC,SAAQC,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,EAAEC,SAAS,QAAO,KAAK;AACvC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIN,QAAQ,EAAE;IAEtBE,SAAS,CAAE,MAAM;MACfI,KAAK,CAACC,QAAQ,CAAC,wBAAwB;MACvCD,KAAK,CAACC,QAAQ,CAAC,oBAAoB;IACrC,CAAC;IAED,IAAIC,KAAI,GAAIT,QAAQ,EAAE;IACtB,MAAMU,SAAQ,GAAIR,QAAQ,CAAC,MAAMK,KAAK,CAACI,OAAO,CAACC,mBAAmB,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACT,IAAG,KAAMI,KAAK,CAACM,MAAM,CAACC,IAAI,CAAC;IAEhH,IAAIC,UAAS,GAAIf,QAAQ,CAAC,MAAMK,KAAK,CAACI,OAAO,CAACO,aAAa,CAAC;IAC5D,MAAMC,SAAQ,GAAI,SAAAA,CAAUC,GAAG,EAAE;MAC/B,IAAGA,GAAG,CAACC,QAAQ,CAAC,MAAM,CAAC,EACrB,OAAOD,GAAE;MAEX,OAAOE,OAAO,CAAC,wBAAuB,GAAIF,GAAG,CAAC;IAChD;IACA,OAAO;MACLb,KAAK;MACLS,IAAI,EAAEN,SAAS;MACfO,UAAU;MACVE;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}