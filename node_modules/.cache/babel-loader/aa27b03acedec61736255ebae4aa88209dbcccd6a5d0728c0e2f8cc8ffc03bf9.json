t, userId) {\r\n      if (comment.usersIdLikes.indexOf(userId) == -1) {\r\n        comment.usersIdLikes.push(userId);\r\n        console.log(comment.usersIdLikes);\r\n      }\r\n      else {\r\n        comment.usersIdLikes.splice(comment.usersIdLikes.findIndex(userId), 1)\r\n      }\r\n    };\r\n\r\n    let putDislike = function (comment, userId) {\r\n      comment.usersIdDislikes.push(userId)\r\n    };\r\n\r\n    let comments = ref([\r\n      {\r\n        user: 'username',\r\n        date: formattedToday,\r\n        stars: 4,\r\n        comment: 'Хороший тест!',\r\n        usersIdLikes: [1, 2],\r\n        usersIdDislikes: [],\r\n      },\r\n    ]);\r\n    return {\r\n      comments,\r\n      putLike,\r\n      putDislike\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAyCA,SAASA,GAAE,QAAS,KAAK;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAI,IAAIC,IAAI,EAAE;IACxB,MAAMC,IAAG,GAAIF,KAAK,CAACG,WAAW,EAAE;IAChC,IAAIC,EAAC,GAAIJ,KAAK,CAACK,QAAQ,EAAC,GAAI,CAAC,EAAE;IAC/B,IAAIC,EAAC,GAAIN,KAAK,CAACO,OAAO,EAAE;IAExB,IAAID,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAC1B,IAAIF,EAAC,GAAI,EAAE,EAAEA,EAAC,GAAI,GAAE,GAAIA,EAAE;IAE1B,MAAMI,cAAa,GAAIF,EAAC,GAAI,GAAE,GAAIF,EAAC,GAAI,GAAE,GAAIF,IAAI;IAEjD,IAAIO,OAAM,GAAI,SAAAA,CAAUC,OAAO,EAAEC,MAAM,EAAE;MACvC,IAAID,OAAO,CAACE,YAAY,CAACC,OAAO,CAACF,MAAM,KAAK,CAAC,CAAC,EAAE;QAC9CD,OAAO,CAACE,YAAY,CAACE,IAAI,CAACH,MAAM,CAAC;QACjCI,OAAO,CAACC,GAAG,CAACN,OAAO,CAACE,YAAY,CAAC;MACnC,OACK;QACHF,OAAO,CAACE,YAAY,CAACK,MAAM,CAACP,OAAO,CAACE,YAAY,CAACM,SAAS,CAACP,MAAM,CAAC,EAAE,CAAC;MACvE;IACF,CAAC;IAED,IAAIQ,UAAS,GAAI,SAAAA,CAAUT,OAAO,EAAEC,MAAM,EAAE;MAC1CD,OAAO,CAACU,eAAe,CAACN,IAAI,CAACH,MAAM;IACrC,CAAC;IAED,IAAIU,QAAO,GAAIxB,GAAG,CAAC,CACjB;MACEyB,IAAI,EAAE,UAAU;MAChBC,IAAI,EAAEf,cAAc;MACpBgB,KAAK,EAAE,CAAC;MACRd,OAAO,EAAE,eAAe;MACxBE,YAAY,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC;MACpBQ,eAAe,EAAE;IACnB,CAAC,CACF,CAAC;IACF,OAAO;MACLC,QAAQ;MACRZ,OAAO;MACPU;IACF;EACF;AACF"},"metada