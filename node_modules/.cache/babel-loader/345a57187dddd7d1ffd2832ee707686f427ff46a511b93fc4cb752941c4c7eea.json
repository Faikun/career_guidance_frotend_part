{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { useStore } from \"vuex\";\nimport { computed } from \"vue\";\nexport default {\n  name: \"ProfOrientationTemplate\",\n  props: ['routeName'],\n  async setup(props) {\n    let store = useStore();\n    await store.dispatch(\"initialQuestionList\");\n    const tests = computed(() => store.getters.getTests);\n    let testQuestionsNum;\n    try {\n      const test = tests.value.filter(el => el.name === routeName.value)[0].questions;\n      let allTest = [];\n      for (let i = 0; i < test.length; i++) {\n        const innerArray = test[i].questions;\n        for (let j = 0; j < innerArray.length; j++) {\n          allTest.push(innerArray[j]);\n          allTest[allTest.length - 1]['isAnswered'] = false;\n          allTest[allTest.length - 1]['name'] = test[i].name;\n        }\n      }\n    } catch (e) {\n      console.log(e.message);\n    }\n    return {\n      testQuestionsNum\n    };\n  }\n};","map":{"version":3,"names":["useStore","computed","name","props","setup","store","dispatch","tests","getters","getTests","testQuestionsNum","test","value","filter","el","routeName","questions","allTest","i","length","innerArray","j","push","e","console","log","message"],"sources":["E:\\Vue\\career_guidance\\src\\components\\ProfOrientationTemplate.vue"],"sourcesContent":["<template>\r\n  <div class=\"prof__details\">\r\n    <div class=\"prof__details-block\">\r\n      <img class=\"prof__details-img\" src=\"../images/png/Check.png\" alt=\"questions\">\r\n      <span>{{ testQuestionsNum ? testQuestionsNum : 'Неизвестное количество' }} вопросов</span>\r\n    </div>\r\n    <div class=\"prof__details-block\">\r\n      <img class=\"prof__details-img\" src=\"../images/png/Time.png\" alt=\"Time\">\r\n      <span>{{ testQuestionsNum ? '20' : 'Неизвестное количество' }} мин</span>\r\n    </div>\r\n  </div><!-- ./prof__details -->\r\n  <button type=\"button\" class=\"btn green\" @click=\"testQuestionsNum ? $router.push({name: 'test', props: {name: routeName}}) : ''\">Начать</button>\r\n</template>\r\n\r\n<script>\r\nimport {useStore} from \"vuex\";\r\nimport {computed} from \"vue\";\r\n\r\nexport default {\r\n  name: \"ProfOrientationTemplate\",\r\n  props: ['routeName'],\r\n  async setup(props) {\r\n    let store = useStore();\r\n    await store.dispatch(\"initialQuestionList\");\r\n\r\n    const tests = computed(() => store.getters.getTests);\r\n    let testQuestionsNum;\r\n\r\n    try {\r\n      const test = tests.value.filter((el) => el.name === routeName.value)[0].questions;\r\n      let allTest = []\r\n\r\n      for (let i = 0; i < test.length; i++) {\r\n        const innerArray = test[i].questions;\r\n        for (let j = 0; j < innerArray.length; j++) {\r\n          allTest.push(innerArray[j]);\r\n          allTest[allTest.length-1]['isAnswered'] = false;\r\n          allTest[allTest.length-1]['name'] = test[i].name;\r\n        }\r\n      }\r\n    }\r\n    catch (e) {\r\n      console.log(e.message)\r\n    }\r\n\r\n    return {\r\n      testQuestionsNum\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAeA,SAAQA,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,QAAO,KAAK;AAE5B,eAAe;EACbC,IAAI,EAAE,yBAAyB;EAC/BC,KAAK,EAAE,CAAC,WAAW,CAAC;EACpB,MAAMC,KAAKA,CAACD,KAAK,EAAE;IACjB,IAAIE,KAAI,GAAIL,QAAQ,EAAE;IACtB,MAAMK,KAAK,CAACC,QAAQ,CAAC,qBAAqB,CAAC;IAE3C,MAAMC,KAAI,GAAIN,QAAQ,CAAC,MAAMI,KAAK,CAACG,OAAO,CAACC,QAAQ,CAAC;IACpD,IAAIC,gBAAgB;IAEpB,IAAI;MACF,MAAMC,IAAG,GAAIJ,KAAK,CAACK,KAAK,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACZ,IAAG,KAAMa,SAAS,CAACH,KAAK,CAAC,CAAC,CAAC,CAAC,CAACI,SAAS;MACjF,IAAIC,OAAM,GAAI,EAAC;MAEf,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIP,IAAI,CAACQ,MAAM,EAAED,CAAC,EAAE,EAAE;QACpC,MAAME,UAAS,GAAIT,IAAI,CAACO,CAAC,CAAC,CAACF,SAAS;QACpC,KAAK,IAAIK,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAID,UAAU,CAACD,MAAM,EAAEE,CAAC,EAAE,EAAE;UAC1CJ,OAAO,CAACK,IAAI,CAACF,UAAU,CAACC,CAAC,CAAC,CAAC;UAC3BJ,OAAO,CAACA,OAAO,CAACE,MAAM,GAAC,CAAC,CAAC,CAAC,YAAY,IAAI,KAAK