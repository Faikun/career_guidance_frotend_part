      },\r\n      {\r\n        category: category.name.Medicine,\r\n        categoryImage: category.img.Medicine,\r\n        name: 'Нейрохирург',\r\n        description: ''\r\n      },\r\n      {\r\n        category: category.name.Management,\r\n        categoryImage: category.img.Management,\r\n        name: 'Менеджмент',\r\n        description: ''\r\n      },\r\n      {\r\n        category: category.name.Art,\r\n        categoryImage:  category.img.Art,\r\n        name: 'Искусство',\r\n        description: ''\r\n      },\r\n      {\r\n        category: category.name.Science,\r\n        categoryImage: category.img.Science,\r\n        name: 'Наука',\r\n        description: ''\r\n      }\r\n    ]\r\n\r\n    const route = useRoute()\r\n    let profs = professionsArray.filter(el => el.name === route.params.name);\r\n    return {\r\n      profs\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAkBA,SAAQA,QAAQ,QAAO,YAAY;AAEnC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAKA,CAAA,EAAG;IACN,IAAIC,QAAO,GAAI;MACbF,IAAI,EAAE;QACJG,EAAE,EAAE,IAAI;QACRC,QAAQ,EAAE,UAAU;QACpBC,OAAO,EAAE,WAAW;QACpBC,UAAU,EAAE,IAAI;QAChBC,GAAG,EAAE,UAAU;QACfC,OAAO,EAAE;MACX,CAAC;MACDC,GAAG,EAAE;QACHN,EAAE,EAAEO,OAAO,CAAC,wBAAwB,CAAC;QACrCN,QAAQ,EAAEM,OAAO,CAAC,8BAA8B,CAAC;QACjDL,OAAO,EAAEK,OAAO,CAAC,6BAA6B,CAAC;QAC/CJ,UAAU,EAAEI,OAAO,CAAC,gCAAgC,CAAC;QACrDH,GAAG,EAAEG,OAAO,CAAC,yBAAyB,CAAC;QACvCF,OAAO,EAAEE,OAAO,CAAC,6BAA6B;MAChD;IACF,CAAC;IACD,IAAIC,gBAAe,GAAI,CACrB;MACET,QAAQ,EAAEA,QAAQ,CAACF,IAAI,CAACG,EAAE;MAC1BS,aAAa,EAAEV,QAAQ,CAACO,GAAG,CAACN,EAAE;MAC9BH,IAAI,EAAE,sBAAsB;MAC5Ba,WAAW,EAAE;IACf,CAAC,EACD;MACEX,QAAQ,EAAEA,QAAQ,CAACF,IAAI,CAACK,OAAO;MAC/BO,aAAa,EAAGV,QAAQ,CAACO,GAAG,CAACJ,OAAO;MACpCL,IAAI,EAAE,WAAW;MACjBa,WAAW,EAAE;IACf,CAAC,EACD;MACEX,QAAQ,EAAEA,QAAQ,CAACF,IAAI,CAACI,QAAQ;MAChCQ,aAAa,EAAEV,QAAQ,CAACO,GAAG,CAACL,QAAQ;MACpCJ,IAAI,EAAE,aAAa;MACnBa,WAAW,EAAE;IACf,CAAC,EACD;MACEX,QAAQ,EAAEA,QAAQ,CAACF,IAAI,CAACM,UAAU;MAClCM,aAAa,EAAEV,QAAQ,CAACO,GAAG