{"ast":null,"code":"import { onMounted, ref } from \"vue\";\nimport store from \"@/store\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  name: \"Edu\",\n  setup() {\n    let dropdownActive = ref(false);\n    let searchDone = ref(false);\n    let university = store.getters.getUniversity;\n    let route = useRoute();\n    let status = ref('Статус: Любой');\n    let area = ref('Область обучения');\n    onMounted(() => searchEdu());\n    let searchEdu = function () {\n      let city = university.filter(el => el.location === route.params.city);\n      if (status.value !== 'Статус: Любой') {\n        city = city.filter(el => el.status === status.value);\n      }\n      if (area.value !== 'Область обучения') {\n        city = city.filter(el => el.tags.contain(area.value));\n      }\n      searchDone.value = true;\n      return city;\n    };\n    return {\n      dropdownActive,\n      searchEdu,\n      searchDone,\n      status,\n      area\n    };\n  }\n};","map":{"version":3,"names":["onMounted","ref","store","useRoute","name","setup","dropdownActive","searchDone","university","getters","getUniversity","route","status","area","searchEdu","city","filter","el","location","params","value","tags","contain"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Edu.vue"],"sourcesContent":["<template>\r\n<div class=\"edu\">\r\n  <h1 class=\"edu__title\">Вузы города {{ $route.params.city }}</h1>\r\n\r\n  <div class=\"edu__search\">\r\n    <div class=\"edu__search-intro\">\r\n      <input type=\"text\" placeholder=\"Введите вуз\">\r\n      <button class=\"button\" type=\"button\" @click=\"searchEdu\">Поиск</button>\r\n    </div><!-- ./edu__search__intro -->\r\n    <div class=\"edu__search-additional\">\r\n      <select class=\"edu__search-block\" v-model=\"status\">\r\n        <option value=\"Статус: Любой\">Статус: Любой</option>\r\n        <option value=\"государственный\">Государственный</option>\r\n        <option value=\"частный\">Частный</option>\r\n      </select><!-- ./edu__search-block -->\r\n      <select class=\"edu__search-block\" v-model=\"area\">\r\n        <option value=\"Область обучения\">Область обучения</option>\r\n        <option value=\"Pedagogy\">Педагогический</option>\r\n        <option value=\"Technology\">Технический</option>\r\n        <option value=\"Medicine\">Медицина</option>\r\n        <option value=\"Engineering\">Инженерия</option>\r\n        <option value=\"Business\">Бизнес</option>\r\n        <option value=\"Natural sciences\">Естествознание</option>\r\n        <option value=\"Economics\">Экономика</option>\r\n        <option value=\"Agriculture\">Сельское хозяйство</option>\r\n        <option value=\"Law\">Закон</option>\r\n        <option value=\"Music\">Музыка</option>\r\n        <option value=\"Arts\">Искусство</option>\r\n        <option value=\"Education\">Образвание</option>\r\n      </select><!-- ./edu__search-block -->\r\n      <div class=\"edu__search-subjects\" @click=\"dropdownActive = !dropdownActive\">\r\n        <span>Предметы ЕНТ</span>\r\n        <img src=\"../images/png/arrow_toButton.png\" alt=\"arrow\">\r\n        <div class=\"edu__dropdown\" :class=\"dropdownActive ? 'active' : ''\" @click.stop>\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"math\">Математика</label>\r\n            <input type=\"checkbox\" id=\"math\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"physics\">Физика</label>\r\n            <input type=\"checkbox\" id=\"physics\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"chemistry \">Химия</label>\r\n            <input type=\"checkbox\" id=\"chemistry \">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"kzHistory\">История Казахстана</label>\r\n            <input type=\"checkbox\" id=\"kzHistory\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"worldHistory\">Всемирная история</label>\r\n            <input type=\"checkbox\" id=\"worldHistory\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"eng\">Английский</label>\r\n            <input type=\"checkbox\" id=\"eng\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"geography\">География</label>\r\n            <input type=\"checkbox\" id=\"geography\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n        </div><!-- ./edu__dropdown -->\r\n      </div><!-- ./edu__search-subjects -->\r\n      <div class=\"edu__checkbox\">\r\n        <label for=\"mildep\">военная кафедра</label>\r\n        <input type=\"checkbox\" id=\"mildep\">\r\n      </div><!--./edu__checkbox -->\r\n      <div class=\"edu__checkbox\">\r\n        <label for=\"hostel\">общежитие</label>\r\n        <input type=\"checkbox\" id=\"hostel\">\r\n      </div><!--./edu__checkbox -->\r\n    </div><!-- ./edu__search-additional -->\r\n  </div><!-- ./edu__search -->\r\n\r\n  <section class=\"edu__results\" v-if=\"searchDone\">\r\n    <ul class=\"edu__results-list\">\r\n      <li class=\"edu__results-top\">\r\n        <span>Найдено: {{ searchEdu().length }}</span>\r\n        <span>Проходной балл</span>\r\n        <span>Бюджетных мест</span>\r\n        <span>Внебюджетных мест</span>\r\n      </li>\r\n      <li class=\"edu__results-block\" v-for=\"edu in searchEdu()\">\r\n        <div class=\"edu__results-data first\">\r\n          <a href=\"#\" class=\"edu__results-link\">\r\n            <img class=\"edu__results-logo\" :src=\"edu.img\" alt=\"logo\">\r\n            <h2>{{ edu.name_ru }}</h2>\r\n          </a>\r\n          <div class=\"edu__results-additional\">\r\n            <span v-if=\"edu.hotel\">Общежитие</span>\r\n            <span v-if=\"edu.military\">Военная кафедра</span>\r\n          </div>\r\n        </div>\r\n        <div class=\"edu__results-data\">\r\n          {{ edu.passScores }}\r\n        </div>\r\n        <div class=\"edu__results-data\">\r\n          {{ edu.budget }}\r\n        </div>\r\n        <div class=\"edu__results-data\">\r\n          {{ edu.offbudget }}\r\n        </div>\r\n      </li>\r\n    </ul>\r\n  </section><!-- ./edu__results -->\r\n</div><!-- ./edu -->\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, ref} from \"vue\";\r\nimport store from \"@/store\";\r\nimport {useRoute} from \"vue-router\";\r\n\r\nexport default {\r\n  name: \"Edu\",\r\n  setup() {\r\n    let dropdownActive = ref(false);\r\n    let searchDone = ref(false);\r\n    let university = store.getters.getUniversity;\r\n    let route = useRoute();\r\n\r\n    let status = ref('Статус: Любой');\r\n    let area = ref('Область обучения');\r\n\r\n    onMounted(() => searchEdu())\r\n\r\n    let searchEdu = function () {\r\n      let city = university.filter((el) => el.location === route.params.city);\r\n      if(status.value !== 'Статус: Любой') {\r\n        city = city.filter((el) => el.status === status.value)\r\n      }\r\n      if(area.value !== 'Область обучения') {\r\n        city = city.filter((el) => el.tags.contain(area.value))\r\n      }\r\n      searchDone.value = true;\r\n      return city\r\n    }\r\n    return {\r\n      dropdownActive,\r\n      searchEdu,\r\n      searchDone,\r\n      status, area\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AA8GA,SAAQA,SAAS,EAAEC,GAAG,QAAO,KAAK;AAClC,OAAOC,KAAI,MAAO,SAAS;AAC3B,SAAQC,QAAQ,QAAO,YAAY;AAEnC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN,IAAIC,cAAa,GAAIL,GAAG,CAAC,KAAK,CAAC;IAC/B,IAAIM,UAAS,GAAIN,GAAG,CAAC,KAAK,CAAC;IAC3B,IAAIO,UAAS,GAAIN,KAAK,CAACO,OAAO,CAACC,aAAa;IAC5C,IAAIC,KAAI,GAAIR,QAAQ,EAAE;IAEtB,IAAIS,MAAK,GAAIX,GAAG,CAAC,eAAe,CAAC;IACjC,IAAIY,IAAG,GAAIZ,GAAG,CAAC,kBAA