{"ast":null,"code":"import { computed, onMounted, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  name: \"AdminAdditional\",\n  setup() {\n    onMounted(async () => {\n      await store.dispatch('setAllTags');\n      await store.dispatch('setAllSubjects');\n    });\n    let tag = ref(''),\n      subject = ref('');\n    const store = useStore();\n    const tags = computed(() => store.getters.getTags);\n    const subjects = computed(() => store.getters.getSubjects);\n    const sendTags = async function () {\n      await axios.post('tags', {\n        name: tag.value\n      }).then(res => {\n        const data = res.response.data;\n        const error = data.error;\n        if (error) {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Упс',\n            text: error,\n            mode: 'danger'\n          });\n        } else {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Успех',\n            text: 'Тег добавлен успешно!',\n            mode: 'success'\n          });\n        }\n      }).catch(error => console.log(error));\n      document.documentElement.scrollTop = 0;\n    };\n    const sendSubjects = async function () {\n      await axios.post('subjects', {\n        name: subject.value\n      });\n    };\n    const removeSubject = async function (id) {\n      await axios.delete('subjects', {\n        id: id\n      });\n    };\n    const removeTag = async function (id) {\n      await axios.delete('tags', {\n        id: id\n      });\n    };\n    return {\n      tags,\n      subjects,\n      sendTags,\n      sendSubjects,\n      removeSubject,\n      removeTag,\n      tag,\n      subject\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","ref","useStore","axios","name","setup","store","dispatch","tag","subject","tags","getters","getTags","subjects","getSubjects","sendTags","post","value","then","res","data","response","error","commit","titleValue","text","mode","catch","console","log","document","documentElement","scrollTop","sendSubjects","removeSubject","id","delete","removeTag"],"sources":["E:\\Vue\\career_guidance\\src\\components\\AdminAdditional.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin__table-wrapper\">\r\n    <div class=\"admin__table-block\">\r\n      <h2 class=\"admin__subtitle\">Добавление тегов</h2>\r\n      <table class=\"table admin__table-additional\">\r\n        <tr>\r\n          <th>Название</th>\r\n          <th>Удалить/Добавить</th>\r\n        </tr>\r\n        <tr v-for=\"tag in tags\" :id=\"tag.id\">\r\n          <td>{{ tag.name }}</td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/remove.png\" alt=\"remove\" @click=\"removeTag\">\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            <input type=\"text\"  v-model.trim.lazy=\"tag\">\r\n          </td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/save.png\" alt=\"save\" @click=\"sendTags\">\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n    <div class=\"admin__table-block\">\r\n      <h2 class=\"admin__subtitle mt50\">Добавление предметов</h2>\r\n      <table class=\"table admin__table-additional\">\r\n        <tr>\r\n          <th>Название</th>\r\n          <th>Удалить/Добавить</th>\r\n        </tr>\r\n        <tr v-for=\"subject in subjects\" :id=\"subject.id\">\r\n          <td>{{ subject.name }}</td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/remove.png\" alt=\"remove\" @click=\"removeSubject\">\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            <input type=\"text\" v-model.trim.lazy=\"subject\">\r\n          </td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/save.png\" alt=\"save\" @click=\"sendSubjects\">\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted, ref} from \"vue\";\r\nimport {useStore} from \"vuex\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"AdminAdditional\",\r\n  setup() {\r\n    onMounted(async () => {\r\n      await store.dispatch('setAllTags')\r\n      await store.dispatch('setAllSubjects')\r\n    })\r\n\r\n    let tag = ref(''),\r\n        subject = ref('');\r\n\r\n    const store = useStore()\r\n    const tags = computed(() => store.getters.getTags)\r\n    const subjects = computed(() => store.getters.getSubjects)\r\n\r\n    const sendTags = async function () {\r\n      await axios.post('tags', {\r\n        name: tag.value\r\n      }).then(res => {\r\n        const data = res.response.data\r\n        const error = data.error;\r\n        if(error) {\r\n          store.commit('setNotificationInfo', { titleValue: 'Упс', text: error, mode: 'danger'} );\r\n        }\r\n        else {\r\n          store.commit('setNotificationInfo', { titleValue: 'Успех', text: 'Тег добавлен успешно!', mode: 'success'} );\r\n        }\r\n      }).catch(error => console.log(error))\r\n      document.documentElement.scrollTop = 0;\r\n    }\r\n\r\n    const sendSubjects = async function () {\r\n      await axios.post('subjects', {\r\n        name: subject.value\r\n      })\r\n    }\r\n\r\n    const removeSubject = async function (id) {\r\n      await axios.delete('subjects', {\r\n        id: id\r\n      })\r\n    }\r\n\r\n    const removeTag = async function (id) {\r\n      await axios.delete('tags', {\r\n        id: id\r\n      })\r\n    }\r\n    return {\r\n      tags, subjects,\r\n      sendTags,sendSubjects,\r\n      removeSubject, removeTag,\r\n      tag, subject\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAoDA,SAAQA,QAAQ,EAAEC,SAAS,EAAEC,GAAG,QAAO,KAAK;AAC5C,SAAQC,QAAQ,QAAO,MAAM;AAC7B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAG;IACNL,S