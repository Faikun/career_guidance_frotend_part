{"ast":null,"code":"import { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\nimport { computed, onMounted } from \"vue\";\nexport default {\n  name: \"Profession\",\n  setup() {\n    let store = useStore();\n    let route = useRoute();\n    onMounted(async () => await store.dispatch('setAllUniversities'));\n    let prof = store.getters.getProfessionsArray.filter(el => el.name === route.params.prof);\n    let university = computed(store.getters.getUniversity);\n    const getImgUrl = function (img) {\n      if (img.includes('http')) return img;\n      return require('@/images/Prof/Certain/' + img);\n    };\n    return {\n      store,\n      prof,\n      university,\n      getImgUrl,\n      university\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useStore","computed","onMounted","name","setup","store","route","dispatch","prof","getters","getProfessionsArray","filter","el","params","university","getUniversity","getImgUrl","img","includes","require"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Profession.vue"],"sourcesContent":["<template>\r\n  <div class=\"profession\">\r\n    <h1 class=\"profession__title\">{{ this.$route.params.prof }}</h1>\r\n    <div class=\"profession__article\">\r\n      <div class=\"profession__author\">Admin</div>\r\n      <div class=\"profession__date\">11 марта 2023</div>\r\n    </div> <!-- ./profession__article -->\r\n    <div class=\"profession__image\">\r\n      <img :src=\"getImgUrl(prof[0].image)\" alt=\"image\">\r\n    </div>\r\n    <div class=\"profession__contents\">\r\n      <h2 class=\"profession__contents-title\">Содержание</h2>\r\n      <ul class=\"profession__contents-list\">\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#description\">Описание</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#whatToDo\">Что нужно делать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#know\">Что нужно знать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#money\">Сколько можно заработать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#study\">Где можно обучиться</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"profession__info\" v-for=\"array in prof\">\r\n      <h2 class=\"profession__info-title\" id=\"description\">Описание</h2>\r\n      <div class=\"profession__info-text\">{{ array.description }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"whatToDo\">Что нужно делать</h2>\r\n      <div class=\"profession__info-text\">{{ array.whatMakes }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"know\">Что нужно знать</h2>\r\n      <div class=\"profession__info-text\">{{ array.whatShouldKnow }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"money\">Сколько можно заработать</h2>\r\n      <div class=\"profession__info-text\">{{ array.money }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"study\">Где можно обучиться</h2>\r\n      <div class=\"profession__info-text\">\r\n        <ul v-if=\"university.filter((el) => el.tags.includes(array.category)).toString()\">\r\n          <li v-for=\"item in university.filter((el) => el.tags.includes(array.category)).map((el) => el.name)\">\r\n            <router-link :to=\"'/university/' + item\">{{ item }}</router-link>\r\n          </li>\r\n        </ul>\r\n        <span v-else>Список пуст</span>\r\n      </div>\r\n    </div><!-- ./profession__info -->\r\n  </div><!-- ./profession -->\r\n</template>\r\n\r\n<script>\r\nimport {useRoute} from \"vue-router\";\r\nimport {useStore} from \"vuex\";\r\nimport {computed, onMounted} from \"vue\";\r\n\r\nexport default {\r\n  name: \"Profession\",\r\n  setup() {\r\n    let store = useStore();\r\n    let route = useRoute();\r\n    onMounted(async () => await store.dispatch('setAllUniversities'))\r\n\r\n    let prof = store.getters.getProfessionsArray.filter((el) => el.name === route.params.prof);\r\n    let university = computed(store.getters.getUniversity);\r\n    const getImgUrl = function (img) {\r\n      if(img.includes('http'))\r\n        return img\r\n\r\n      return require('@/images/Prof/Certain/' + img);\r\n    }\r\n    return {\r\n      store,\r\n      prof,\r\n      university,\r\n      getImgUrl,\r\n      university\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAqDA,SAAQA,QAAQ,QAAO,YAAY;AACnC,SAAQC,QAAQ,QAAO,MAAM;AAC7B,SAAQC,QAAQ,EAAEC,SAAS,QAAO,KAAK;AAEvC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIL,QAAQ,EAAE;IACtB,IAAIM,KAAI,GAAIP,QAAQ,EAAE;IACtBG,SAAS,CAAC,YAAY,MAAMG,KAAK,CAACE,QAAQ,CAAC,oBAAoB,CAAC;IAEhE,IAAIC,IAAG,GAAIH,KAAK,CAACI,OAAO,CAACC,mBAAmB,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACT,IAAG,KAAMG,KAAK,CAACO,MAAM,CAACL,IAAI,CAAC;IAC1F,IAAIM,UAAS,GAAIb,QAAQ,CAACI,KAAK,CAACI,OAAO,CAACM,aAAa,CAAC;IACtD,MAAMC,SAAQ,GAAI,SAAAA,CAAUC,GAAG,EAAE;MAC/B,IAAGA,GAAG,CAACC,QAAQ,CAAC,MAAM,CAAC,EACrB,OAAOD,GAAE;MAEX,OAAOE,OAAO,CAAC,wBAAuB,GAAIF,GAAG,CAAC;IAChD;IACA,OAAO;MACLZ,KAAK;MACLG,IAAI;MACJM,UAAU;MACVE,SAAS;MACTF;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}