{"ast":null,"code":"import { computed, onMounted, ref } from \"vue\";\nimport { useStore } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  name: \"AdminAdditional\",\n  setup() {\n    onMounted(async () => {\n      await store.dispatch('setAllTags');\n      await store.dispatch('setAllSubjects');\n    });\n    let tag = ref(''),\n      subject = ref('');\n    const store = useStore();\n    const tags = computed(() => store.getters.getTags);\n    const subjects = computed(() => store.getters.getSubjects);\n    const validate = function (data, text) {\n      if (data.length() > 3) {\n        return true;\n      } else {\n        store.commit('setNotificationInfo', {\n          titleValue: 'Упс',\n          text: text,\n          mode: 'danger'\n        });\n        return false;\n      }\n    };\n    const sendTags = async function () {\n      if (validate(tag.value, 'Название тега должно быть больше трех символов в длину')) {\n        await axios.post('tags', {\n          name: tag.value\n        }).then(async res => {\n          const data = res.data;\n          const error = data.error;\n          if (error) {\n            store.commit('setNotificationInfo', {\n              titleValue: 'Упс',\n              text: error,\n              mode: 'danger'\n            });\n          } else {\n            store.commit('setNotificationInfo', {\n              titleValue: 'Успех',\n              text: 'Тег добавлен успешно!',\n              mode: 'success'\n            });\n            tag.value = '';\n            await store.dispatch('setAllTags');\n          }\n        }).catch(error => console.log(error));\n        document.documentElement.scrollTop = 0;\n      }\n    };\n    const sendSubjects = async function () {\n      await axios.post('subjects', {\n        name: subject.value\n      }).then(async res => {\n        const data = res.data;\n        const error = data.error;\n        if (error) {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Упс',\n            text: error,\n            mode: 'danger'\n          });\n        } else {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Успех',\n            text: 'Предмет добавлен успешно!',\n            mode: 'success'\n          });\n          await store.dispatch('setAllSubjects');\n          subject.value = '';\n        }\n      }).catch(error => console.log(error));\n      document.documentElement.scrollTop = 0;\n    };\n    const removeSubject = async function (id) {\n      await axios.delete('subjects/' + id).then(async res => {\n        const data = res.data;\n        const error = data.error;\n        if (error) {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Упс',\n            text: error,\n            mode: 'danger'\n          });\n        } else {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Успех',\n            text: 'Предмет успешно удален!',\n            mode: 'success'\n          });\n          await store.dispatch('setAllSubjects');\n        }\n      }).catch(error => console.log(error));\n      document.documentElement.scrollTop = 0;\n    };\n    const removeTag = async function (id) {\n      await axios.delete('tags/' + id).then(async res => {\n        const data = res.data;\n        const error = data.error;\n        if (error) {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Упс',\n            text: error,\n            mode: 'danger'\n          });\n        } else {\n          store.commit('setNotificationInfo', {\n            titleValue: 'Успех',\n            text: 'Тег успешно удален!',\n            mode: 'success'\n          });\n          await store.dispatch('setAllTags');\n        }\n      }).catch(error => console.log(error));\n      document.documentElement.scrollTop = 0;\n    };\n    return {\n      tags,\n      subjects,\n      sendTags,\n      sendSubjects,\n      removeSubject,\n      removeTag,\n      tag,\n      subject\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","ref","useStore","axios","name","setup","store","dispatch","tag","subject","tags","getters","getTags","subjects","getSubjects","validate","data","text","length","commit","titleValue","mode","sendTags","value","post","then","res","error","catch","console","log","document","documentElement","scrollTop","sendSubjects","removeSubject","id","delete","removeTag"],"sources":["E:\\Vue\\career_guidance\\src\\components\\AdminAdditional.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin__table-wrapper\">\r\n    <div class=\"admin__table-block\">\r\n      <h2 class=\"admin__subtitle\">Добавление тегов</h2>\r\n      <table class=\"table admin__table-additional\">\r\n        <tr>\r\n          <th>Название</th>\r\n          <th>Удалить/Добавить</th>\r\n        </tr>\r\n        <tr v-for=\"tag in tags\" :id=\"tag.id\">\r\n          <td>{{ tag.name }}</td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/remove.png\" alt=\"remove\" @click=\"removeTag(tag.id)\">\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            <input type=\"text\"  v-model.trim.lazy=\"tag\">\r\n          </td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/save.png\" alt=\"save\" @click=\"sendTags\">\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n    <div class=\"admin__table-block\">\r\n      <h2 class=\"admin__subtitle mt50\">Добавление предметов</h2>\r\n      <table class=\"table admin__table-additional\">\r\n        <tr>\r\n          <th>Название</th>\r\n          <th>Удалить/Добавить</th>\r\n        </tr>\r\n        <tr v-for=\"subject in subjects\" :id=\"subject.id\">\r\n          <td>{{ subject.name }}</td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/remove.png\" alt=\"remove\" @click=\"removeSubject(subject.id)\">\r\n          </td>\r\n        </tr>\r\n        <tr>\r\n          <td>\r\n            <input type=\"text\" v-model.trim.lazy=\"subject\">\r\n          </td>\r\n          <td>\r\n            <img class=\"admin__table-edit\" src=\"@/images/png/save.png\" alt=\"save\" @click=\"sendSubjects\">\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted, ref} from \"vue\";\r\nimport {useStore} from \"vuex\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"AdminAdditional\",\r\n  setup() {\r\n    onMounted(async () => {\r\n      await store.dispatch('setAllTags')\r\n      await store.dispatch('setAllSubjects')\r\n    })\r\n\r\n    let tag = ref(''),\r\n        subject = ref('');\r\n\r\n    const store = useStore()\r\n    const tags = computed(() => store.getters.getTags)\r\n    const subjects = computed(() => store.getters.getSubjects)\r\n\r\n    const validate = function (data, text) {\r\n      if(data.length() > 3) {\r\n        return true\r\n      }\r\n      else {\r\n        store.commit('setNotificationInfo', {titleValue: 'Упс', text: text, mode: 'danger'});\r\n        return false\r\n      }\r\n    }\r\n\r\n    const sendTags = async function () {\r\n      if (validate(tag.value, 'Название тега должно быть больше трех символов в длину')) {\r\n        await axios.post('tags', {\r\n          name: tag.value\r\n        }).then(async res => {\r\n          const data = res.data\r\n          const error = data.error;\r\n          if (error) {\r\n            store.commit('setNotificationInfo', {titleValue: 'Упс', text: error, mode: 'danger'});\r\n          } else {\r\n            store.commit('setNotificationInfo', {titleValue: 'Успех', text: 'Тег добавлен успешно!', mode: 'success'});\r\n            tag.value = ''\r\n            await store.dispatch('setAllTags')\r\n          }\r\n        }).catch(error => console.log(error))\r\n        document.documentElement.scrollTop = 0;\r\n      }\r\n    }\r\n\r\n    const sendSubjects = async function () {\r\n      await axios.post('subjects', {\r\n        name: subject.value\r\n      }).then(async res => {\r\n        const data = res.data\r\n        const error = data.error;\r\n        if (error) {\r\n          store.commit('setNotificationInfo', {titleValue: 'Упс', text: error, mode: 'danger'});\r\n        } else {\r\n          store.commit('setNotificationInfo', {\r\n            titleValue: 'Успех',\r\n            text: 'Предмет добавлен успешно!',\r\n            mode: 'success'\r\n          });\r\n          await store.dispatch('setAllSubjects')\r\n          subject.value = ''\r\n        }\r\n      }).catch(error => console.log(error))\r\n      document.documentElement.scrollTop = 0;\r\n    }\r\n\r\n    const removeSubject = async function (id) {\r\n      await axios.delete('subjects/' + id).then(async res => {\r\n        const data = res.data\r\n        const error = data.error;\r\n        if (error) {\r\n          store.commit('setNotificationInfo', {titleValue: 'Упс', text: error, mode: 'danger'});\r\n        } else {\r\n          store.commit('setNotificationInfo', {titleValue: 'Успех', text: 'Предмет успешно удален!', mode: 'success'});\r\n          await store.dispatch('setAllSubjects')\r\n        }\r\n      }).catch(error => console.log(error))\r\n      document.documentElement.scrollTop = 0;\r\n    }\r\n\r\n    const removeTag = async function (id) {\r\n      await axios.delete('tags/' + id).then(async res => {\r\n        const data = res.data\r\n        const error = data.error;\r\n        if (error) {\r\n          store.commit('setNotificationInfo', {titleValue: 'Упс', text: error, mode: 'danger'});\r\n        }\r\n        else {\r\n          store.commit('setNotificationInfo', {titleValue: 'Успех', text: 'Тег успешно удален!', mode: 'success'});\r\n          await store.dispatch('setAllTags')\r\n        }\r\n      }).catch(error => console.log(error))\r\n      document.documentElement.scrollTop = 0;\r\n    }\r\n    return {\r\n      tags, subjects,\r\n      sendTags,sendSubjects,\r\n      removeSubject, removeTag,\r\n      tag, subject\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAoDA,SAAQA,QAAQ,EAAEC,SAAS,EAAEC,GAAG,QAAO,KAAK;AAC5C,SAAQC,QAAQ,QAAO,MAAM;AAC7B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAG;IACNL,SAAS,CAAC,YAAY;MACpB,MAAMM,KAAK,CAACC,QAAQ,CAAC,YAAY;MACjC,MAAMD,KAAK,CAACC,QAAQ,CAAC,gBAAgB;IACvC,CAAC;IAED,IAAIC,GAAE,GAAIP,GAAG,CAAC,EAAE,CAAC;MACbQ,OAAM,GAAIR,GAAG,CAAC,EAAE,CAAC;IAErB,MAAMK,KAAI,GAAIJ,QAAQ,EAAC;IACvB,MAAMQ,IAAG,GAAIX,QAAQ,CAAC,MAAMO,KAAK,CAACK,OAAO,CAACC,OAAO;IACjD,MAAMC,QAAO,GAAId,QAAQ,CAAC,MAAMO,KAAK,CAACK,OAAO,CAACG,WAAW;IAEzD,MAAMC,QAAO,GAAI,SAAAA,CAAUC,IAAI,EAAEC,IAAI,EAAE;MACrC,IAAGD,IAAI,CAACE,MAAM,EAAC,GAAI,CAAC,EAAE;QACpB,OAAO,IAAG;MACZ,OACK;QACHZ,KAAK,CAACa,MAAM,CAAC,qBAAqB,EAAE;UAACC,UAAU,EAAE,KAAK;UAAEH,IAAI,EAAEA,IAAI;UAAEI,IAAI,EAAE;QAAQ,CAAC,CAAC;QACpF,OAAO,KAAI;MACb;IACF;IAEA,MAAMC,QAAO,GAAI,eAAAA,CAAA,EAAkB;MACjC,IAAIP,QAAQ,CAACP,GAAG,CAACe,KAAK,EAAE,wDAAwD,CAAC,EAAE;QACjF,MAAMpB,KAAK,CAACqB,IAAI,CAAC,MAAM,EAAE;UACvBpB,IAAI,EAAEI,GAAG,CAACe;QACZ,CAAC,CAAC,CAACE,IAAI,CAAC,MAAMC,GAAE,IAAK;UACnB,MAAMV,IAAG,GAAIU,GAAG,CAACV,IAAG;UACpB,MAAMW,KAAI,GAAIX,IAAI,CAACW,KAAK;UACxB,IAAIA,KAAK,EAAE;YACTrB,KAAK,CAACa,MAAM,CAAC,qBAAqB,EAAE;cAACC,UAAU,EAAE,KAAK;cAAEH,IAAI,EAAEU,KAAK;cAAEN,IAAI,EAAE;YAAQ,CAAC,CAAC;UACvF,OAAO;YACLf,KAAK,CAACa,MAAM,CAAC,qBAAqB,EAAE;cAACC,UAAU,EAAE,OAAO;cAAEH,IAAI,EAAE,uBAAuB;cAAEI,IAAI,EAAE;YAAS,CAAC,CAAC;YAC1Gb,GAAG,CAACe,KAAI,GAAI,EAAC;YACb,MAAMjB,KAAK,CAACC,QAAQ,CAAC,YAAY;UACnC;QACF,CAAC,CAAC,CAACqB,KAAK,CAACD,KAAI,IAAKE,OAAO,CAACC,GAAG,CAACH,KAAK,CAAC;QACpCI,QAAQ,CAACC,eAAe,CAACC,SAAQ,GAAI,CAAC;MACxC;IACF;IAEA,MAAMC,YAAW,GAAI,eAAAA,CAAA,EAAkB;MACrC,MAAM/B,KAAK,CAACqB,IAAI,CAAC,UAAU,EAAE;QAC3BpB,IAAI,EAAEK,OAAO,CAACc;MAChB,CAAC,CAAC,CAACE,IAAI,CAAC,MAAMC,GAAE,IAAK;QACnB,MAAMV,IAAG,GAAIU,GAAG,CAACV,IAAG;QACpB,MAAMW,KAAI,