ate>\r\n\r\n<script>\r\nimport {useStore} from \"vuex\";\r\n\r\nexport default {\r\n  name: \"SearchUniversity\",\r\n  props: ['requestText'],\r\n  setup(props) {\r\n    const store = useStore();\r\n    const edu = store.getters.getUniversity;\r\n\r\n    let searchEdu = edu.filter((el) => el.name_ru.includes(props.requestText));\r\n    console.log('function searchEdu: ',searchEdu)\r\n\r\n    return {\r\n      searchEdu: searchEdu.value\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAmCA,SAAQA,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE,CAAC,aAAa,CAAC;EACtBC,KAAKA,CAACD,KAAK,EAAE;IACX,MAAME,KAAI,GAAIJ,QAAQ,EAAE;IACxB,MAAMK,GAAE,GAAID,KAAK,CAACE,OAAO,CAACC,aAAa;IAEvC,IAAIC,SAAQ,GAAIH,GAAG,CAACI,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACC,OAAO,CAACC,QAAQ,CAACV,KAAK,CAACW,WAAW,CAAC,CAAC;IAC1EC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAACP,SAAS;IAE5C,OAAO;MACLA,SAAS,EAAEA,SAAS,CAACQ;IACvB;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}