{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, onMounted, reactive, ref } from \"vue\";\nimport router from \"@/router\";\nimport store from \"@/store\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"Professions\",\n  setup() {\n    let searchProf = ref();\n    const store = useStore();\n    const category = computed(() => store.getters.getProfCategory);\n    const categoryName = Object.values(Object.values(category.value)[0]);\n    const categoryImg = Object.values(Object.values(category.value)[1]);\n    let output = store.commit('getFullCategory', {\n      categoryName,\n      categoryImg\n    });\n    let openProfessionList = function (prof) {\n      router.push({\n        name: 'Exact_Professions',\n        params: {\n          name: prof.name\n        }\n      });\n    };\n    onMounted(() => searchProfButton());\n    let searchProfButton = function (clicked) {\n      if (clicked) {\n        console.log('clicked');\n        output = output.filter(el => el.name.toLowerCase().includes(searchProf.value.toLowerCase()));\n        console.log(output);\n      }\n    };\n    return {\n      searchProf,\n      openProfessionList,\n      output,\n      searchProfButton,\n      categoryName,\n      categoryImg\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","reactive","ref","router","store","useStore","name","setup","searchProf","category","getters","getProfCategory","categoryName","Object","values","value","categoryImg","output","commit","openProfessionList","prof","push","params","searchProfButton","clicked","console","log","filter","el","toLowerCase","includes"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Professions.vue"],"sourcesContent":["<template>\r\n<div class=\"professions\">\r\n  <h1 class=\"professions__title\">Профессии</h1>\r\n\r\n  <div class=\"search\">\r\n    <input\r\n        class=\"search__input black\"\r\n        type=\"text\"\r\n        v-model.trim.lazy=\"searchProf\"\r\n        placeholder=\"Введите профессию\"\r\n        @keyup.enter = searchProfButton(true)>\r\n    <img class=\"search__icon\" src=\"../images/png/Search.png\" alt=\"icon\" @click=\"searchProfButton()\">\r\n  </div><!-- ./search -->\r\n\r\n  <div class=\"professions__wrapper\">\r\n    <div class=\"test__block\" v-for=\"prof in output\" :key=\"output\" @click=\"openProfessionList(prof)\">\r\n      <img class=\"test__block-img\" :src=\"prof.img\" alt=\"image\">\r\n      <span class=\"tests__name\">{{ prof.name }}</span>\r\n    </div>\r\n  </div><!-- ./professions__wrapper -->\r\n\r\n</div><!-- ./professions -->\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted, reactive, ref} from \"vue\";\r\nimport router from \"@/router\";\r\nimport store from \"@/store\";\r\nimport {useStore} from \"vuex\";\r\n\r\nexport default {\r\n  name: \"Professions\",\r\n  setup() {\r\n    let searchProf = ref();\r\n    const store = useStore();\r\n\r\n    const category = computed(() => store.getters.getProfCategory);\r\n\r\n    const categoryName = Object.values(Object.values(category.value)[0])\r\n    const categoryImg = Object.values(Object.values(category.value)[1])\r\n\r\n    let output = store.commit('getFullCategory', {categoryName, categoryImg})\r\n\r\n    let openProfessionList = function (prof) {\r\n      router.push({name: 'Exact_Professions', params: {name: prof.name}});\r\n    }\r\n\r\n    onMounted(() => searchProfButton());\r\n\r\n    let searchProfButton = function (clicked) {\r\n      if(clicked) {\r\n        console.log('clicked')\r\n        output = output.filter((el) => el.name.toLowerCase().includes(searchProf.value.toLowerCase()));\r\n        console.log(output)\r\n      }\r\n    };\r\n\r\n\r\n    return {\r\n      searchProf,\r\n      openProfessionList,\r\n      output,\r\n      searchProfButton,\r\n      categoryName, categoryImg\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAyBA,SAAQA,QAAQ,EAAEC,SAAS,EA