{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { computed, onMounted, reactive, ref } from \"vue\";\nimport router from \"@/router\";\nimport store from \"@/store\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"Professions\",\n  setup() {\n    let searchProf = ref();\n    const store = useStore();\n    const category = computed(() => store.getters.getProfCategory);\n    const categoryName = Object.values(Object.values(category.value)[0]);\n    const categoryImg = Object.values(Object.values(category.value)[1]);\n    let output = [];\n    for (let i = 0; i < categoryName.length; i++) {\n      output.push({\n        name: categoryName[i],\n        img: categoryImg[i]\n      });\n    }\n    let openProfessionList = function (prof) {\n      router.push({\n        name: 'Exact_Professions',\n        params: {\n          name: prof.name\n        }\n      });\n    };\n    onMounted(() => searchProfButton());\n    let searchProfButton = function (clicked) {\n      output = [];\n      for (let i = 0; i < categoryName.length; i++) {\n        output.push({\n          name: categoryName[i],\n          img: categoryImg[i]\n        });\n      }\n      console.log('mounted', output);\n      if (clicked) {\n        output = output.filter(el => el.name.toLowerCase().includes(searchProf.value.toLowerCase()));\n      }\n    };\n    return {\n      searchProf,\n      openProfessionList,\n      output,\n      searchProfButton,\n      categoryName,\n      categoryImg\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","reactive","ref","router","store","useStore","name","setup","searchProf","category","getters","getProfCategory","categoryName","Object","values","value","categoryImg","output","i","length","push","img","openProfessionList","prof","params","searchProfButton","clicked","console","log","filter","el","toLowerCase","includes"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Professions.vue"],"sourcesContent":["<template>\r\n<div class=\"professions\">\r\n  <h1 class=\"professions__title\">Профессии</h1>\r\n\r\n  <div class=\"search\">\r\n    <input\r\n        class=\"search__input black\"\r\n        type=\"text\"\r\n        v-model.trim.lazy=\"searchProf\"\r\n        placeholder=\"Введите профессию\"\r\n        @keyup.enter = searchProfButton(true)>\r\n    <img class=\"search__icon\" src=\"../images/png/Search.png\" alt=\"icon\" @click=\"searchProfButton()\">\r\n  </div><!-- ./search -->\r\n\r\n  <div class=\"professions__wrapper\">\r\n    <div class=\"test__block\" v-for=\"prof in output\" :key=\"prof.name\" @click=\"openProfessionList(prof)\">\r\n      <img class=\"test__block-img\" :src=\"prof.img\" alt=\"image\">\r\n      <span class=\"tests__name\">{{ prof.name }}</span>\r\n    </div>\r\n  </div><!-- ./professions__wrapper -->\r\n\r\n</div><!-- ./professions -->\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted, reactive, ref} from \"vue\";\r\nimport router from \"@/router\";\r\nimport store from \"@/store\";\r\nimport {useStore} from \"vuex\";\r\n\r\nexport default {\r\n  name: \"Professions\",\r\n  setup() {\r\n    let searchProf = ref();\r\n    const store = useStore();\r\n\r\n    const category = computed(() => store.getters.getProfCategory);\r\n\r\n    const categoryName = Object.values(Object.values(category.value)[0])\r\n    const categoryImg = Object.values(Object.values(category.value)[1])\r\n\r\n    let output = []\r\n    for (let i = 0; i < categoryName.length; i++) {\r\n      output.push({\r\n        name: categoryName[i],\r\n        img: categoryImg[i]\r\n      })\r\n    }\r\n\r\n    let openProfessionList = function (prof) {\r\n      router.push({name: 'Exact_Professions', params: {name: prof.name}});\r\n    }\r\n\r\n    onMounted(() => searchProfButton());\r\n\r\n    let searchProfButton = function (clicked) {\r\n      output = [];\r\n      for (let i = 0; i < categoryName.length; i++) {\r\n        output.push({\r\n          name: categoryName[i],\r\n          img: categoryImg[i]\r\n        })\r\n      }\r\n      console.log('mounted', output)\r\n\r\n      if(clicked) {\r\n        output = output.filter((el) => el.name.toLowerCase().includes(searchProf.value.toLowerCase()));\r\n      }\r\n    };\r\n\r\n\r\n    return {\r\n      searchProf,\r\n      openProfessionList,\r\n      output,\r\n      searchProfButton,\r\n      categoryName, categoryImg\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAyBA,SAAQA,QAAQ,EAAEC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACtD,OAAOC,MAAK,MAAO,UAAU;AAC7B,OAAOC,KAAI,MAAO,SAAS;AAC3B,SAAQC,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACN,IAAIC,UAAS,GAAIN,GAAG,EAAE;IACtB,MAAME,KAAI,GAAIC,QAAQ,EAAE;IAExB,MAAMI,QAAO,GAAIV,QAAQ,CAAC,MAAMK,KAAK,CAACM,OAAO,CAACC,eAAe,CAAC;IAE9D,MAAMC,YAAW,GAAIC,MAAM,CAACC,MAAM,CAACD,MAAM,CAACC,MAAM,CAACL,QAAQ,CAACM,KAAK,CAAC,CAAC,CAAC,CAAC;IACnE,MAAMC,WAAU,GAAIH,MAAM,CAACC,MAAM,CAACD,MAAM,CAACC,MAAM,CAACL,QAAQ,CAACM,KAAK,CAAC,CAAC,CAAC,CAAC;IAElE,IAAIE,MAAK,GAAI,EAAC;IACd,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIN,YAAY,CAACO,MAAM,EAAED,CAAC,EAAE,EAAE;MAC5CD,MAAM,CAACG,IAAI,CAAC;QACVd,IAAI,EAAEM,YAAY,CAACM,CAAC,CAAC;QACrBG,GAAG,EAAEL,WAAW,CAACE,CAAC;MACpB,CAAC;IACH;IAEA,IAAII,kBAAiB,GAAI,SAAAA,CAAUC,IAAI,EAAE;MACvCpB,MAAM,CAACiB,IAAI,CAAC;QAACd,IAAI,EAAE,mBAAmB;QAAEkB,MAAM,EAAE;UAAClB,IAAI,EAAEiB,IAAI,CAACjB;QAAI;MAAC,CAAC,CAAC;IACrE;IAEAN,SAAS,CAAC,MAAMyB,gBAAgB,EAAE,CAAC;IAEnC,IAAIA,gBAAe,GAAI,SAAAA,CAAUC,OAAO,EAAE;MACxCT,MAAK,GAAI,EAAE;MACX,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAIN,YAAY,CAACO,MAAM,EAAED,CAAC,EAAE,EAAE;QAC5CD,MAAM,CAACG,IAAI,CAAC;UACVd,IAAI,EAAEM,YAAY,CAACM,CAAC,CAAC;UACrBG,GAAG,EAAEL,WAAW,CAACE,CAAC;QACpB,CAAC;MACH;MACAS,OAAO,CAACC,GAAG,CAAC,SAAS,EAAEX,MAAM;MAE7B,IAAGS,OAAO,EAAE;QACVT,MAAK,GAAIA,MAAM,CAACY,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACxB,IAAI,CAACyB,WAAW,EAAE,CAACC,QAAQ,CAACxB,UAAU,CAACO,KAAK,CAACgB,WAAW,EAAE,CAAC,CAAC;MAChG;IACF,CAAC;IAGD,OAAO;MACLvB,UAAU;MACVc