 profMakes.value,\r\n          whatShouldKnow: profKnows.value,\r\n          money:profMoney.value\r\n        }\r\n        store.commit('setNotificationInfo', { titleValue: 'Успех', text: 'Вы успешно добавили новую профессию', mode: 'success'} );\r\n        profCategory.value = '';\r\n        profImage.value = '';\r\n        profName.value = '';\r\n        profDescription.value = '';\r\n        profMakes.value = '';\r\n        profKnows.value = '';\r\n        profMoney.value = '';\r\n        document.documentElement.scrollTop = 0;\r\n        store.commit('addProf', data);\r\n      }\r\n      else {\r\n        alert('Заполните все поля')\r\n      }\r\n    }\r\n    return {\r\n      profName, profCategory, profImage, profDescription, profMakes, profKnows, profMoney,\r\n      addProf,\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAmDA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,QAAO,MAAO,0BAA0B;AAC/C,SAAQC,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,EAAC;IACvB,IAAII,QAAO,GAAIN,GAAG,CAAC,EAAE,CAAC;MAClBO,YAAW,GAAIP,GAAG,CAAC,IAAI,CAAC;MACxBQ,SAAQ,GAAIR,GAAG,CAAC,EAAE,CAAC;MACnBS,eAAc,GAAIT,GAAG,CAAC,EAAE,CAAC;MACzBU,SAAQ,GAAIV,GAAG,CAAC,EAAE,CAAC;MACnBW,SAAQ,GAAIX,GAAG,CAAC,EAAE,CAAC;MACnBY,SAAQ,GAAIZ,GAAG,CAAC,EAAE,CAAC;IAEvB,MAAMa,WAAU,GAAKC,IAAI,IAAKA,IAAI,CAACC,KAAI,KAAM,EAAC,IAAKD,IAAI,CAACC,KAAK,CAACC,MAAK,GAAI;IAEvE,MAAMC,OAAM,GAAI,SAAAA,CAAA,EAAY;MAC1B,IACIJ,WAAW,CAACP,QAAQ,KAAKO,WAAW,CAACN,YAAY,KACjDM,WAAW,CAACL,SAAS,KAAKK,WAAW,CAACJ,eAAe,KACrDI,WAAW,CAACH,SAAS,KAAKG,WAAW,CAACF,SAAS,KAC/CE,WAAW,CAACD,SAAS,CAAC,EAC1B;QACE,MAAMM,IAAG,GAAI;UACXjB,QAAQ,EAAEA,QAAQ,CAACkB,KAAK,CAAClB,QAAQ,CAACE,IAAI,CAACI,YAAY,CAACQ,KAAK,CAAC;UAC1DK,aAAa,EAAEnB,QAAQ,CAACkB,KAAK,CAAClB,QAAQ,CAACoB,GAAG,CAACd,YAAY,CAACQ,KAAK,CAAC;UAC9DO,KAAK,EAAEd,SAAS,CAACO,KAAK;UACtBZ,IAAI,EAAEG,QAAQ,CAACS,KAAK;UACpBQ,WAAW,EAAEd,eAAe,CAACM,KAAK;UAClCS,SAAS,EAAEd,SAAS,CAACK,KAAK;UAC1BU,cAAc,EAAEd,SAAS,CAACI,KAAK;UAC