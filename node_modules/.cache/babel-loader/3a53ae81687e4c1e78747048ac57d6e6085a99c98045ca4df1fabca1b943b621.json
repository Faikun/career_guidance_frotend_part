ntValue.isAnswered ? 1 : 0), 0))\r\n    let nextQuestion = function (question, answer) {\r\n      if(checkQuestionCompleted.value + 1 !== test.value.length) {\r\n        this.test[question].isAnswered = true;\r\n        this.test[question].chosenAnswer = answer;\r\n\r\n        if (firstUnanswered.value > -1) {\r\n          questionNum.value = firstUnanswered.value;\r\n        }\r\n        else {\r\n          questionNum.value++;\r\n        }\r\n      }\r\n      else {\r\n        this.test[question].isAnswered = true\r\n        console.log(this.test)\r\n        setTimeout(() => { router.push('/result'); }, 250);\r\n      }\r\n    }\r\n    return {\r\n      questionNum,\r\n      test,\r\n      nextQuestion,\r\n      checkQuestionCompleted\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";AAwCA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC,OAAOC,MAAK,MAAO,UAAU;AAC7B,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAAQC,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEH;EAAS,CAAC;EACxBI,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,KAAKA,CAAA,EAAG;IACN,IAAIC,WAAU,GAAIR,GAAG,CAAC,CAAC,CAAC;IACxB,MAAMS,KAAI,GAAIN,QAAQ,EAAE;IACxB,MAAMO,KAAI,GAAIX,QAAQ,CAAC,MAAMU,KAAK,CAACE,OAAO,CAACC,QAAQ,CAAC;IACpD,MAAMC,IAAG,GAAIH,KAAK,CAACI,KAAK,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACZ,IAAG,KAAMA,IAAI,CAACU,KAAK,CAAC,CAAC,CAAC,CAAC,CAACG,SAAQ;IAE3E,IAAIC,eAAc,GAAInB,QAAQ,CAAC,MAAMc,IAAI,CAACC,KAAK,CAACK,SAAS,CAACN,IAAG,IAAKA,IAAI,CAACO,UAAS,KAAM,KAAK,CAAC,CAAC;IAC7F,IAAIC,sBAAqB,GAAItB,QAAQ,CAAC,MAAMc,IAAI,CAACC,KAAK,CAACQ,MAAM,CAAC,CAACC,WAAW,EAAEC,YAAY,KAAKD,WAAU,IAAKC,YAAY,CAACJ,UAAS,GAAI,IAAI,CAAC,CAAC,EAAE,CAAC,CAAC;IAChJ,IAAIK,YAAW,GAAI,SAAAA,CAAUC,QAAQ,EAAEC,MAAM,EAAE;MAC7C,IAAGN,sBAAsB,CAACP,KAAI,GAAI,MAAMD,IAAI,CAACC,KAAK,CAACc,MAAM,EAAE;QACzD,IAAI,CAACf,IAAI,CAACa,QAAQ,CAAC,CAACN,UAAS,GAAI,IAAI;QACrC,IAAI,CAACP,IAAI,CAACa,QAAQ,CAAC,CAACG,YAAW,GAAIF,MAAM;QAEzC,IAAIT,eAAe,CAACJ,KAAI,GAAI,CAAC,CAAC,EAAE;UAC9BN,WAAW,CAACM,KAAI,GAAII,eAAe,CAACJ,KAAK;QAC3C,OACK;UACHN,