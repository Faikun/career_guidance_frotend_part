{"ast":null,"code":"import { useRoute } from \"vue-router\";\nimport { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"ProfOrientation\",\n  setup() {\n    const route = useRoute();\n    const store = useStore();\n    const routeName = computed(() => route.params.name);\n    const tests = computed(() => store.getters.getTests);\n    const testQuestionsNum = tests.value.filter(el => el.name.toString() === routeName.toString());\n    return {\n      routeName,\n      testQuestionsNum,\n      tests\n    };\n  }\n};","map":{"version":3,"names":["useRoute","computed","useStore","name","setup","route","store","routeName","params","tests","getters","getTests","testQuestionsNum","value","filter","el","toString"],"sources":["E:\\Vue\\career_guidance\\src\\views\\ProfOrientation.vue"],"sourcesContent":["<template>\r\n  <div class=\"prof\">\r\n    <div class=\"prof__info\">\r\n      <div class=\"prof__title\">{{ routeName }} {{ tests }}</div>\r\n      <div class=\"prof__text\">По прохождению данного теста, вы узнаете к чему вы предрасположены,\r\n        таким образом, данный тест поможет вам с определением вашей профориентации</div>\r\n      <div class=\"prof__details\">\r\n        <div class=\"prof__details-block\">\r\n          <img class=\"prof__details-img\" src=\"../images/png/Check.png\" alt=\"questions\">\r\n          <span>{{ testQuestionsNum }} вопроса</span>\r\n        </div>\r\n        <div class=\"prof__details-block\">\r\n          <img class=\"prof__details-img\" src=\"../images/png/Time.png\" alt=\"Time\">\r\n          <span>20 мин</span>\r\n        </div>\r\n      </div><!-- ./prof__details -->\r\n      <button type=\"button\" class=\"btn green\" @click=\"$router.push('/test')\">Начать</button>\r\n    </div><!-- ./prof__info -->\r\n    <div class=\"prof__image\">\r\n      <video autoplay loop src=\"../images/gif/Main.mp4\"></video>\r\n    </div><!-- ./prof__image -->\r\n  </div><!-- ./prof -->\r\n</template>\r\n\r\n<script>\r\nimport {useRoute} from \"vue-router\";\r\nimport {computed} from \"vue\";\r\nimport {useStore} from \"vuex\";\r\n\r\nexport default {\r\n  name: \"ProfOrientation\",\r\n  setup() {\r\n    const route = useRoute();\r\n    const store = useStore();\r\n    const routeName = computed(() => route.params.name);\r\n\r\n    const tests = computed(() => store.getters.getTests);\r\n    const testQuestionsNum = tests.value.filter((el) => el.name.toString() === routeName.toString());\r\n    return {\r\n      routeName,\r\n      testQuestionsNum,\r\n      tests\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAyBA,SAAQA,QAAQ,QAAO,YAAY;AACnC,SAAQC,QAAQ,QAAO,KAAK;AAC5B,SAAQC,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,QAAQ,EAAE;IACxB,MAAMM,KAAI,GAAIJ,QAAQ,EAAE;IACxB,MAAMK,SAAQ,GAAIN,QAAQ,CAAC,MAAMI,KAAK,CAACG,MAAM,CAACL,IAAI,CAAC;IAEnD,MAAMM,KAAI,GAAIR,QAAQ,CAAC,MAAMK,KAAK,CAACI,OAAO,CAACC,QAAQ,CAAC;IACpD,MAAMC,gBAAe,GAAIH,KAAK,CAACI,KAAK,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACZ,IAAI,CAACa,QAAQ,EAAC,KAAMT,SAAS,CAACS,QAAQ,EAAE,CAAC;IAChG,OAAO;MACLT,SAAS;MACTK,gBAAgB;MAChBH;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}