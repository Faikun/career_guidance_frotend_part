{"ast":null,"code":"import { useRoute } from \"vue-router\";\nimport { useStore } from \"vuex\";\nexport default {\n  name: \"Profession\",\n  setup() {\n    let store = useStore();\n    let route = useRoute();\n    let prof = store.getters.getProfessionsArray.filter(el => el.name === route.params.prof);\n    let university = store.getters.getUniversity;\n    const getImgUrl = function (img) {\n      return require(img);\n    };\n    console.log(university);\n    return {\n      store,\n      prof,\n      university,\n      getImgUrl\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useStore","name","setup","store","route","prof","getters","getProfessionsArray","filter","el","params","university","getUniversity","getImgUrl","img","require","console","log"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Profession.vue"],"sourcesContent":["<template>\r\n  <div class=\"profession\">\r\n    <h1 class=\"profession__title\">{{ this.$route.params.prof }}</h1>\r\n    <div class=\"profession__article\">\r\n      <div class=\"profession__author\">Admin</div>\r\n      <div class=\"profession__date\">11 марта 2023</div>\r\n    </div> <!-- ./profession__article -->\r\n    <div class=\"profession__image\">\r\n      <img :src=\"require(`${prof[0].image}`)\" alt=\"image\">\r\n    </div>\r\n    <div class=\"profession__contents\">\r\n      <h2 class=\"profession__contents-title\">Содержание</h2>\r\n      <ul class=\"profession__contents-list\">\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#description\">Описание</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#whatToDo\">Что нужно делать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#know\">Что нужно знать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#money\">Сколько можно заработать</a>\r\n        </li>\r\n        <li class=\"profession__contents-item\">\r\n          <a href=\"#study\">Где можно обучиться</a>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"profession__info\" v-for=\"array in prof\">\r\n      <h2 class=\"profession__info-title\" id=\"description\">Описание</h2>\r\n      <div class=\"profession__info-text\">{{ array.description }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"whatToDo\">Что нужно делать</h2>\r\n      <div class=\"profession__info-text\">{{ array.whatMakes }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"know\">Что нужно знать</h2>\r\n      <div class=\"profession__info-text\">{{ array.whatShouldKnow }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"money\">Сколько можно заработать</h2>\r\n      <div class=\"profession__info-text\">{{ array.money }}</div>\r\n      <h2 class=\"profession__info-title\" id=\"study\">Где можно обучиться</h2>\r\n      <div class=\"profession__info-text\">\r\n        <ul v-if=\"university.filter((el) => el.tags.includes(array.category)).toString()\">\r\n          <li v-for=\"item in university.filter((el) => el.tags.includes(array.category)).map((el) => el.name_ru)\">\r\n            <router-link to=\"\">{{ item }}</router-link>\r\n          </li>\r\n        </ul>\r\n        <span v-else>Список пуст</span>\r\n      </div>\r\n    </div><!-- ./profession__info -->\r\n  </div><!-- ./profession -->\r\n</template>\r\n\r\n<script>\r\nimport {useRoute} from \"vue-router\";\r\nimport {useStore} from \"vuex\";\r\n\r\nexport default {\r\n  name: \"Profession\",\r\n  setup() {\r\n    let store = useStore();\r\n    let route = useRoute();\r\n    let prof = store.getters.getProfessionsArray.filter((el) => el.name === route.params.prof);\r\n    let university = store.getters.getUniversity;\r\n    const getImgUrl = function (img) {\r\n      return require(img);\r\n    }\r\n    console.log(university)\r\n    return {\r\n      store,\r\n      prof,\r\n      university,\r\n      getImgUrl\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAqDA,SAAQA,QAAQ,QAAO,YAAY;AACnC,SAAQC,QAAQ,QAAO,MAAM;AAE7B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAKA,CAAA,EAAG;IACN,IAAIC,KAAI,GAAIH,QAAQ,EAAE;IACtB,IAAII,KAAI,GAAIL,QAAQ,EAAE;IACtB,IAAIM,IAAG,GAAIF,KAAK,CAACG,OAAO,CAACC,mBAAmB,CAACC,MAAM,CAAEC,EAAE,IAAKA,EAAE,CAACR,IAAG,KAAMG,KAAK,CAACM,MAAM,CAACL,IAAI,CAAC;IAC1F,IAAIM,UAAS,GAAIR,KAAK,CAACG,OAAO,CAACM,aAAa;IAC5C,MAAMC,SAAQ,GAAI,SAAAA,CAAUC,GAAG,EAAE;MAC/B,OAAOC,OAAO,CAACD,GAAG,CAAC;IACrB;IACAE,OAAO,CAACC,GAAG,CAACN,UAAU;IACtB,OAAO;MACLR,KAAK;MACLE,IAAI;MACJM,UAAU;MACVE;IACF;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}