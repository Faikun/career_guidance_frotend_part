{"ast":null,"code":"import { computed, onMounted, reactive, ref } from \"vue\";\nimport store from \"@/store\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  name: \"Edu\",\n  setup() {\n    let dropdownActive = ref(false);\n    let searchDone = ref(false);\n    let university = store.getters.getUniversity;\n    let route = useRoute();\n    let edu = ref('');\n    let status = ref('Статус: Любой');\n    let area = ref('Область обучения');\n    let subjects = reactive({\n      math: false,\n      physics: false,\n      chemistry: false,\n      worldHistory: false,\n      english: false,\n      geography: false,\n      audioVisual: false\n    });\n    let hotel = ref(false),\n      military = ref(false);\n    onMounted(() => searchEdu());\n    let findCity = computed(() => university.filter(el => el.location === route.params.city));\n    let findSubVal = computed(() => Object.values(subjects));\n    let searchEdu = function (isSearch) {\n      let city = findCity.value;\n      const subjectValues = findSubVal.value;\n      if (isSearch) {\n        city = city.filter(el => el.name_ru.contains(edu.value));\n      } else {\n        if (status.value !== 'Статус: Любой') city = city.filter(el => el.status === status.value);\n        if (area.value !== 'Область обучения') city = city.filter(el => el.tags.includes(area.value));\n        if (military.value) city = city.filter(el => el.military === true);\n        if (hotel.value) city = city.filter(el => el.hotel === true);\n        if (subjectValues.filter(el => el === true).toString()) {\n          if (subjects.math) city = city.filter(el => el.subjects.includes('математика'));\n          if (subjects.chemistry) city = city.filter(el => el.subjects.includes('химия'));\n          if (subjects.physics) city = city.filter(el => el.subjects.includes('физика'));\n          if (subjects.worldHistory) city = city.filter(el => el.subjects.includes('всемирная история'));\n          if (subjects.english) city = city.filter(el => el.subjects.includes('английский'));\n          if (subjects.geography) city = city.filter(el => el.subjects.includes('география'));\n          if (subjects.audioVisual) city = city.filter(el => el.subjects.includes('Аудиовизуальное искусство'));\n        }\n      }\n      searchDone.value = true;\n      return city;\n    };\n    return {\n      dropdownActive,\n      searchEdu,\n      edu,\n      searchDone,\n      status,\n      area,\n      subjects,\n      hotel,\n      military\n    };\n  }\n};","map":{"version":3,"names":["computed","onMounted","reactive","ref","store","useRoute","name","setup","dropdownActive","searchDone","university","getters","getUniversity","route","edu","status","area","subjects","math","physics","chemistry","worldHistory","english","geography","audioVisual","hotel","military","searchEdu","findCity","filter","el","location","params","city","findSubVal","Object","values","isSearch","value","subjectValues","name_ru","contains","tags","includes","toString"],"sources":["E:\\Vue\\career_guidance\\src\\views\\Edu.vue"],"sourcesContent":["<template>\r\n<div class=\"edu\">\r\n  <h1 class=\"edu__title\">Вузы города {{ $route.params.city }}</h1>\r\n\r\n  <div class=\"edu__search\">\r\n    <div class=\"edu__search-intro\">\r\n      <input type=\"text\" placeholder=\"Введите вуз\" v-model=\"edu\">\r\n      <button class=\"button\" type=\"button\" @click=\"searchEdu(true)\">Поиск</button>\r\n    </div><!-- ./edu__search__intro -->\r\n    <div class=\"edu__search-additional\">\r\n      <select class=\"edu__search-block\" v-model=\"status\">\r\n        <option value=\"Статус: Любой\">Статус: Любой</option>\r\n        <option value=\"государственный\">Государственный</option>\r\n        <option value=\"частный\">Частный</option>\r\n      </select><!-- ./edu__search-block -->\r\n      <select class=\"edu__search-block\" v-model=\"area\">\r\n        <option value=\"Область обучения\">Область обучения</option>\r\n        <option value=\"Pedagogy\">Педагогический</option>\r\n        <option value=\"Technology\">Технический</option>\r\n        <option value=\"Medicine\">Медицина</option>\r\n        <option value=\"Engineering\">Инженерия</option>\r\n        <option value=\"Business\">Бизнес</option>\r\n        <option value=\"Natural sciences\">Естествознание</option>\r\n        <option value=\"Economics\">Экономика</option>\r\n        <option value=\"Agriculture\">Сельское хозяйство</option>\r\n        <option value=\"Law\">Закон</option>\r\n        <option value=\"Music\">Музыка</option>\r\n        <option value=\"Arts\">Искусство</option>\r\n        <option value=\"Education\">Образование</option>\r\n      </select><!-- ./edu__search-block -->\r\n      <div class=\"edu__search-subjects\" @click=\"dropdownActive = !dropdownActive\">\r\n        <span>Предметы ЕНТ</span>\r\n        <img src=\"../images/png/arrow_toButton.png\" alt=\"arrow\">\r\n        <div class=\"edu__dropdown\" :class=\"dropdownActive ? 'active' : ''\" @click.stop>\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"math\">Математика</label>\r\n            <input type=\"checkbox\" id=\"math\" v-model=\"subjects.math\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"physics\">Физика</label>\r\n            <input type=\"checkbox\" id=\"physics\"  v-model=\"subjects.physics\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"chemistry\">Химия</label>\r\n            <input type=\"checkbox\" id=\"chemistry\" v-model=\"subjects.chemistry\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"worldHistory\">Всемирная история</label>\r\n            <input type=\"checkbox\" id=\"worldHistory\" v-model=\"subjects.worldHistory\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"eng\">Английский</label>\r\n            <input type=\"checkbox\" id=\"eng\" v-model=\"subjects.english\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"geography\">География</label>\r\n            <input type=\"checkbox\" id=\"geography\" v-model=\"subjects.geography\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n          <div class=\"edu__dropdown-item\">\r\n            <label for=\"audioVisual\">Аудиовизуальное искусство</label>\r\n            <input type=\"checkbox\" id=\"audioVisual\" v-model=\"subjects.audioVisual\">\r\n          </div><!-- ./edu__dropdown-item -->\r\n        </div><!-- ./edu__dropdown -->\r\n      </div><!-- ./edu__search-subjects -->\r\n      <div class=\"edu__checkbox\">\r\n        <label for=\"mildep\">военная кафедра</label>\r\n        <input type=\"checkbox\" id=\"mildep\" v-model=\"military\">\r\n      </div><!--./edu__checkbox -->\r\n      <div class=\"edu__checkbox\">\r\n        <label for=\"hostel\">общежитие</label>\r\n        <input type=\"checkbox\" id=\"hostel\" v-model=\"hotel\">\r\n      </div><!--./edu__checkbox -->\r\n    </div><!-- ./edu__search-additional -->\r\n  </div><!-- ./edu__search -->\r\n\r\n  <section class=\"edu__results\" v-if=\"searchDone\">\r\n    <ul class=\"edu__results-list\">\r\n      <li class=\"edu__results-top\">\r\n        <span>Найдено: {{ searchEdu().length }}</span>\r\n        <span>Проходной балл</span>\r\n        <span>Бюджетных мест</span>\r\n        <span>Внебюджетных мест</span>\r\n      </li>\r\n      <li class=\"edu__results-block\" v-for=\"edu in searchEdu(false)\">\r\n        <div class=\"edu__results-data first\">\r\n          <a href=\"#\" class=\"edu__results-link\">\r\n            <img class=\"edu__results-logo\" :src=\"edu.img\" alt=\"logo\">\r\n            <h2>{{ edu.name_ru }}</h2>\r\n          </a>\r\n          <div class=\"edu__results-additional\">\r\n            <span v-if=\"edu.hotel\">Общежитие</span>\r\n            <span v-if=\"edu.military\">Военная кафедра</span>\r\n          </div>\r\n        </div>\r\n        <div class=\"edu__results-data\">\r\n          {{ edu.passScores }}\r\n        </div>\r\n        <div class=\"edu__results-data\">\r\n          {{ edu.budget }}\r\n        </div>\r\n        <div class=\"edu__results-data\">\r\n          {{ edu.offbudget }}\r\n        </div>\r\n      </li>\r\n    </ul>\r\n  </section><!-- ./edu__results -->\r\n</div><!-- ./edu -->\r\n</template>\r\n\r\n<script>\r\nimport {computed, onMounted, reactive, ref} from \"vue\";\r\nimport store from \"@/store\";\r\nimport {useRoute} from \"vue-router\";\r\n\r\nexport default {\r\n  name: \"Edu\",\r\n  setup() {\r\n    let dropdownActive = ref(false);\r\n    let searchDone = ref(false);\r\n    let university = store.getters.getUniversity;\r\n    let route = useRoute();\r\n\r\n    let edu = ref('');\r\n    let status = ref('Статус: Любой');\r\n    let area = ref('Область обучения');\r\n    let subjects = reactive({\r\n      math: false,\r\n      physics: false,\r\n      chemistry: false,\r\n      worldHistory: false,\r\n      english: false,\r\n      geography: false,\r\n      audioVisual: false\r\n    });\r\n    let hotel = ref(false),\r\n        military = ref(false);\r\n\r\n    onMounted(() => searchEdu());\r\n\r\n    let findCity = computed(() => university.filter((el) => el.location === route.params.city));\r\n    let findSubVal = computed(() => Object.values(subjects));\r\n\r\n    let searchEdu = function (isSearch) {\r\n      let city = findCity.value;\r\n      const subjectValues = findSubVal.value;\r\n\r\n      if(isSearch) {\r\n        city = city.filter((el) => el.name_ru.contains(edu.value));\r\n      }\r\n      \r\n      else {\r\n        if(status.value !== 'Статус: Любой')\r\n          city = city.filter((el) => el.status === status.value)\r\n\r\n        if(area.value !== 'Область обучения')\r\n          city = city.filter((el) => el.tags.includes(area.value))\r\n\r\n        if(military.value)\r\n          city = city.filter((el) => el.military === true)\r\n\r\n        if(hotel.value)\r\n          city = city.filter((el) => el.hotel === true)\r\n\r\n        if(subjectValues.filter((el) => el === true).toString()) {\r\n          if(subjects.math)\r\n            city = city.filter((el) => el.subjects.includes('математика'))\r\n\r\n          if(subjects.chemistry)\r\n            city = city.filter((el) => el.subjects.includes('химия'))\r\n\r\n          if(subjects.physics)\r\n            city = city.filter((el) => el.subjects.includes('физика'))\r\n\r\n          if(subjects.worldHistory)\r\n            city = city.filter((el) => el.subjects.includes('всемирная история'))\r\n\r\n          if(subjects.english)\r\n            city = city.filter((el) => el.subjects.includes('английский'))\r\n\r\n          if(subjects.geography)\r\n            city = city.filter((el) => el.subjects.includes('география'))\r\n\r\n          if(subjects.audioVisual)\r\n            city = city.filter((el) => el.subjects.includes('Аудиовизуальное искусство'))\r\n        }\r\n      }\r\n\r\n      searchDone.value = true;\r\n      return city\r\n    }\r\n    return {\r\n      dropdownActive,\r\n      searchEdu, edu,\r\n      searchDone,\r\n      status, area, subjects,\r\n      hotel, military\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AA8GA,SAAQA,QAAQ,EAAEC,SAAS,EAAEC,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACtD,OAAOC,KAAI,MAAO,SAAS;AAC3B,SAAQC,QAAQ,QAAO,YAAY;AAEnC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAKA,CAAA,EAAG;IACN,IAAIC,cAAa,GAAIL,GAAG,CAAC,KAAK,CAAC;IAC/B,IAAIM,UAAS,GAAIN,GAAG,CAAC,KAAK,CAAC;IAC3B,IAAIO,UAAS,GAAIN,KAAK,CAACO,OAAO,CAACC,aAAa;IAC5C,IAAIC,KAAI,GAAIR,QAAQ,EAAE;IAEtB,IAAIS,GAAE,GAAIX,GAAG,CAAC,EAAE,CAAC;IACjB,IAAIY,MAAK,GAAIZ,GAAG,CAAC,eAAe,CAAC;IACjC,IAAIa,IAAG,GAAIb,GAAG,CAAC,kBAAkB,CAAC;IAClC,IAAIc,QAAO,GAAIf,QAAQ,CAAC;MACtBgB,IAAI,EAAE,KAAK;MAC