{"ast":null,"code":"import { computed, ref } from \"vue\";\nimport store from \"@/store\";\nimport Notification from \"@/components/notification.vue\";\nexport default {\n  name: \"TheNavbar\",\n  components: {\n    Notification\n  },\n  setup() {\n    const storeGetters = computed(() => store.getters);\n    let search = ref('');\n    let account = ref('');\n    let searchActive = ref(false);\n    let toggle = ref(false);\n    let exitAcc = function () {\n      store.commit('setAuth');\n      account.value = !account.value;\n      location.reload();\n    };\n    let showAccBlock = function () {\n      if (store.getters.getAuth) {\n        account.value = !account.value;\n      } else {\n        store.commit('toggleOpenedAuth');\n        document.body.style.overflow = \"hidden\";\n      }\n    };\n    return {\n      search,\n      searchActive,\n      toggle,\n      account,\n      showAccBlock,\n      store,\n      storeGetters,\n      exitAcc\n    };\n  }\n};","map":{"version":3,"names":["computed","ref","store","Notification","name","components","setup","storeGetters","getters","search","account","searchActive","toggle","exitAcc","commit","value","location","reload","showAccBlock","getAuth","document","body","style","overflow"],"sources":["E:\\Vue\\career_guidance\\src\\components\\TheNavbar.vue"],"sourcesContent":["<template>\r\n  <div class=\"nav\">\r\n    <div class=\"container\">\r\n      <div class=\"nav__wrapper\">\r\n        <div class=\"nav__logo\">\r\n          <img src=\"../images/png/logo.png\" alt=\"logo\">\r\n        </div><!-- ./nav__logo -->\r\n\r\n        <nav class=\"nav__links\" :class=\"toggle ? 'shown' : ''\">\r\n          <router-link to=\"/tests\" class=\"nav__link\">Тесты</router-link>\r\n          <div class=\"nav__link dropdown\">\r\n            <span>Выбрать профессию</span>\r\n            <img class=\"nav__arrow\" src=\"../images/png/arrow_toButton.png\" alt=\"arrow\">\r\n\r\n            <div class=\"dropdown__content\">\r\n              <a href=\"#\">Тесты на выбор профессии </a>\r\n              <a href=\"#\">Все профессии</a>\r\n              <router-link to=\"/faq\">FAQ: Как выборать профессию</router-link>\r\n            </div>\r\n          </div><!-- ./nav__link -->\r\n          <div class=\"nav__link dropdown\">\r\n            <span>Поступить в вуз</span>\r\n            <img class=\"nav__arrow\" src=\"../images/png/arrow_toButton.png\" alt=\"arrow\">\r\n\r\n            <div class=\"dropdown__content\">\r\n              <a href=\"#\">Вузы Алматы</a>\r\n              <a href=\"#\">Вузы Нур-Султана</a>\r\n              <a href=\"#\">Вузы Шымкента</a>\r\n              <a href=\"#\">Вузы в вашем городе</a>\r\n            </div>\r\n          </div><!-- ./nav__link -->\r\n        </nav><!-- ./nav__links -->\r\n\r\n        <div class=\"burger__item\" @click=\"toggle = !toggle\">\r\n          <button class=\"burger\" :class=\"toggle ? 'shown' : ''\" type=\"button\">\r\n            <span class=\"burger-item\">Menu</span>\r\n          </button>\r\n        </div>\r\n\r\n        <div class=\"nav__right\">\r\n          <div class=\"nav__search\" @click=\"searchActive = !searchActive\">\r\n            <img src=\"../images/png/Search.png\" alt=\"Search\">\r\n          </div>\r\n          <div class=\"nav__account\">\r\n            <img src=\"../images/png/Account.png\" alt=\"Account\" @click=\"showAccBlock\">\r\n            <div class=\"dropdown__account\" :class=\"account ? 'active' : ''\">\r\n              <a href=\"#\">Аккаунт</a>\r\n              <a href=\"#\">История</a>\r\n              <a href=\"#\" @click=\"exitAcc\">Выход</a>\r\n            </div>\r\n          </div>\r\n        </div><!-- ./nav__right -->\r\n      </div><!-- ./nav__wrapper -->\r\n\r\n      <transition name=\"fadeHeight\">\r\n        <div class=\"search\" v-if=\"searchActive\">\r\n          <input class=\"search__input\" type=\"text\" v-model.trim=\"search\" placeholder=\"Введите тест, профессию или вуз\">\r\n          <img class=\"search__icon\" src=\"../images/png/Search.png\" alt=\"icon\">\r\n        </div><!-- ./search -->\r\n      </transition>\r\n\r\n    </div><!-- ./container -->\r\n  </div><!-- ./nav -->\r\n\r\n  <div class=\"container\">\r\n    <notification\r\n        title=\"Title\"\r\n        text=\"Text\"\r\n    ></notification>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {computed, ref} from \"vue\";\r\nimport store from \"@/store\";\r\nimport Notification from \"@/components/notification.vue\";\r\n\r\nexport default {\r\n  name: \"TheNavbar\",\r\n  components: {Notification},\r\n  setup() {\r\n    const storeGetters = computed(() => store.getters);\r\n    let search = ref('');\r\n    let account = ref('');\r\n    let searchActive = ref(false);\r\n    let toggle = ref(false);\r\n\r\n    let exitAcc = function () {\r\n      store.commit('setAuth')\r\n      account.value = !account.value;\r\n      location.reload();\r\n    }\r\n\r\n    let showAccBlock = function () {\r\n      if (store.getters.getAuth) {\r\n        account.value = !account.value;\r\n      }\r\n      else {\r\n        store.commit('toggleOpenedAuth');\r\n        document.body.style.overflow = \"hidden\";\r\n      }\r\n    }\r\n    return {\r\n      search,\r\n      searchActive,\r\n      toggle,\r\n      account,\r\n      showAccBlock,\r\n      store, storeGetters,\r\n      exitAcc\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .fadeHeight-enter-active,\r\n  .fadeHeight-leave-active {\r\n    transition: all 0.2s;\r\n    max-height: 230px;\r\n  }\r\n  .fadeHeight-enter,\r\n  .fadeHeight-leave-to\r\n  {\r\n    opacity: 0;\r\n    max-height: 0px;\r\n  }\r\n\r\n</style>\r\n"],"mappings":"AA0EA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAACF;EAAY,CAAC;EAC1BG,KAAKA,CAAA,EAAG;IACN,MAAMC,YAAW,GAAIP,QAAQ,CAAC,MAAME,KAAK,CAACM,OAAO,CAAC;IAClD,IAAIC,MAAK,GAAIR,GAAG,CAAC,EAAE,CAAC;IACpB,IAAIS,OAAM,GAAIT,GAAG,CAAC